>line('Application successfully validated');\n	$this->load->view('front/status',$data);\n}\n\nfunction cancelRedeem($token)\n{\n	$result = $this->bitinka_model->cancelRedeem($token);\n	if($result)\n	{\n		echo \"success\";\n	}\n	else\n	{\n		echo \"failure\";\n	}\n}\n\nfunction resendRedeem($token)\n{\n	echo $result = $this->bitinka_model->resendRedeem($token);\n}\n/* Crypto Currency Section Ends Here */\n\nfunction cronupdateReceivedamount() // cronjob for deposit\n{\n	$result = $this->bitinka_model->updateReceivedamount();\n	if($result)\n	{\n	echo \"success\";\n	}\n	else\n	{\n	echo \"failure\";\n	}\n}\n\nfunction cronforOrders() // cronjob for orders\n{\n	$result = $this->bitinka_model->cronforOrdersmodel();\n	if($result)\n	{\n		echo \"success\";\n	}\n	else\n	{\n		echo \"failure\";\n	}\n}\n\nfunction cronforcopay()\n{\n	$row = get_data('admin',array('id'=>1))->row_array();\n	extract($row);\n	$bitcoin_row 	= $this->bitinka_model->fetchWallet('bitcoin');    // fetch bitinka wallet credentials\n	$bitcoin_username	=	$bitcoin_row->username;\n	$bitcoin_password	=	$bitcoin_row->password;\n	$bitcoin_portnumber = 	$bitcoin_row->portnumber;\n	//$bitinka  = new jsonRPCClient(\"http://$bitcoin_username:$bitcoin_password@127.0.0.1:$bitcoin_portnumber/\");\n	$isvalid = $bitinka->getinfo();\n	$walletbalance = $isvalid['balance'];\n	if($copay_limit < $walletbalance)  // 5 < 5.5\n	{\n		$ant = $walletbalance-$copay_limit; // 5.5-5 = 0.5 \n		$amount 	= 	(float)$ant;\n\n		$datas['copay_address'] = $copay_address;\n		$datas['amount']  		= $amount;\n		$result  = insert_data('copay_money',$datas);\n		$id = $this->db->insert_id();\n\n		$transaction_id = $bitinka->sendtoaddress($copay_address,$amount);\n		if($transaction_id)\n		{\n			$updatas = array('transaction_id'=>$transaction_id);\n			update_data('copay_money',$updatas,array('id'=>$id));\n		}\n	}\n	else\n	{\n		echo \"rest\";\n	}\n	// $data['value'] = rand();\n	// update_data('test',$data,array('id'=>'1'));\n}\n\nfunction mapping() \n{\n	$type 	      = $this->input->post('type');\n	$freshorderid = $this->input->post('freshorderid');\n	\n	$result = $this->bitinka_model->mapping($freshorderid);\n	if($result==\"empty\")\n	{\n		echo \"failure\";\n	}\n	else\n	{\n		echo \"success_\".$result;\n	}    \n}\n\nfunction createBuyorder()\n{\n	$amount = $this->input->post('amount');\n	$secondCurrency = $this->input->post('secondCurrency');\n	$price = $this->input->post('price');\n	$total = $amount*$price;\n	$check_userid	=	$this->session->userdata('client_userid'); \n	// $multicoin_client_mailid	=	$this->native_session->get('multicoin_client_mailid');  \n	if(($check_userid==\"\") || $amount==0 || $price==0 || $amount==\"\" || $price==\"\")\n	{   \n	   echo \"login\";\n	}\n	else\n	{\n		$balance = $this->bitinka_model->fetchuserbalancebyId($check_userid,$secondCurrency);\n		if($total<=$balance)\n		{\n			echo $result = $this->bitinka_model->createBuyordermodel();\n		}\n		else\n		{\n			echo \"balance\";\n		}\n	}\n	// $result = $this->bitinka_model->createBuyordermodel();\n}\n\nfunction createSellorder()\n{\n	$amount = $this->input->post('amount');\n	$firstCurrency = $this->input->post('firstCurrency');\n	$price = $this->input->post('price');\n	$total = $this->input->post('total');\n	$check_userid	=	$this->session->userdata('client_userid');  \n	// $multicoin_client_mailid	=	$this->native_session->get('multicoin_client_mailid');  \n	if(($check_userid==\"\") || $amount==0 || $price==0 || $amount==\"\" || $price==\"\")\n	{   \n	   echo \"login\";\n	}\n	else\n	{\n		$balance = $this->bitinka_model->fetchuserbalancebyId($check_userid,$firstCurrency);\n		if($amount<=$balance)\n		{\n			// echo $result = $this->multicoin_model->createBuyordermodel();\n			echo $result = $this->bitinka_model->createSellordermodel();\n		}\n		else\n		{\n			echo \"balance\";\n		}\n	}\n	// echo $result = $this->bitinka_model->createSellordermodel();\n}\n\nfunction createTrades()\n{\n	$result = $this->bitinka_model->createTradesmodel();\n}\n\nfunction postComment()\n{\n	$inputpost 	= trim($this->input->post('inputpost'));\n	$disallowed = array('damn','bull','bullshit','suck','fuck','shit','fucking','sucking','idiot','stupid','f-u-c-k','s-u-c-k','fu-ck','su-ck');\n	$string 	= word_censor($inputpost, $disallowed, 'Beep!');\n	if (strstr($string, 'Beep!') !== false)\n	{ \n		echo \"error\";   \n	}\n	else\n	{\n		$result	= $this->bitinka_model->postCommentmodel();\n	}\n}\n\nfunction refreshChat()\n{\n	$result	=	$this->bitinka_model->refreshChatmodel();\n	if($result)\n	{\n	foreach($result as $chaty)\n	{\n		$username 	= $chaty->username;\n		$message 	= $chaty->message;\n?>\n<div class=\"cls_chat\"><span><?php echo $username; ?>:</span> <?php echo $message; ?></div>\n<?php	}	} else { echo \"No Records Found\"; }\n}\n\nfunction insertPMdetails()\n{\n	echo $this->bitinka_model->insertPMdetails();\n}\nfunction insertDMdetails()\n{\n	echo $this->bitinka_model->insertDMdetails();\n}\nfunction notification()\n{\n   $client_mailid        =        $this->session->userdata('client_mailid'); \n   $client_userid        =        $this->session->userdata('client_userid'); \n   if(($client_mailid==\"\") && ($client_userid==\"\") )\n   {   \n	   $this->session->set_userdata('login_error','Please login here.');      \n	   redirect('bitinka/home','refresh');   \n   }\n   else\n   {\n		$this->bitinka_model->update_noti();\n	   $data['result']=$this->bitinka_model->fetch_notif();\n	   $this->load->view('front/notifications',$data);        \n   }\n}\n\nfunction remove_notif($notif_id)\n{\n    $remove = $this->bitinka_model->remove_notif_model($notif_id);\n    redirect('bitinka/notification','refresh');    \n}\n\nfunction close_active_order($id)\n{\n	$client_mailid		=	$this->session->userdata('client_mailid'); \n	$client_userid		=	$this->session->userdata('client_userid'); \n	if(($client_mailid==\"\") && ($client_userid==\"\") )\n	{   \n		redirect('bitinka/home','refresh'); 	 \n	}\n	else\n	{ \n		$this->session->set_userdata('sessionCloseorder',$id);   \n		redirect('bitinka/closeActiveorder','referesh');  \n	}\n}\n\nfunction closeActiveorder()\n{\n   	$client_mailid		=	$this->session->userdata('client_mailid'); \n	$client_userid		=	$this->session->userdata('client_userid'); \n	if(($client_mailid==\"\") && ($client_userid==\"\") )\n	{   \n		$this->session->set_userdata('login_error','Please login here.');      \n		redirect('bitinka/home','refresh');   \n	}\n	else\n	{	\n		$id	=	$this->session->userdata('sessionCloseorder'); \n		$res_order = $this->bitinka_model->remove_active_model($id);\n		redirect('bitinka/home','refresh');    \n	}\n}\n\nfunction chatlog()\n{\n	$this->load->view('front/chatlog');\n}\n\nfunction api_documentation() {\n	$len = $this->session->userdata('language');\n	switch ($len) {\n		case 'spanish':\n			$this->load->view('front/api_documentation');\n			break;\n		case 'portuguese':\n			$this->load->view('front/api_documentation_portuguese');\n			break;\n		default:\n			$this->load->view('front/api_documentation');\n			break;\n	}\n}\n\nfunction api_inkapay() {\n	$len = $this->session->userdata('language');\n	switch ($len) {\n		case 'spanish':\n			$this->load->view('front/api_inkapay_spa');\n			break;\n		case 'portuguese':\n			$this->load->view('front/api_inkapay_port');\n			break;\n		default:\n			$this->load->view('front/api_inkapay_eng');\n			break;\n	}\n}\n\nfunction search()\n{\n	$array = $this->uri->uri_to_assoc(3);\n	//api key empty or not\n	if(isset($array['api'],$array))\n	{\n		$api_key	= $array['api'];\n	}\n	else\n	{\n		echo \"<h3 align='center'><font color='red'>Please Provide API Key</font></h3>\";\n	}\n	if(isset($array['secret'],$array))\n	{\n		$secret	= $array['secret'];\n	}\n	else\n	{\n		echo \"<h3 align='center'><font color='red'>Please Provide Secret Key</font></h3>\";\n	}\n	if(isset($array['method'],$array))\n	{\n		$method	= $array['method'];\n	}\n	else\n	{\n		echo \"<h3 align='center'><font color='red'>Please Provide Any Method</font></h3>\";\n	}\n	//checking whether the api is assigned for the user\n	if(isset($api_key) && isset($secret) && isset($method))\n	{\n		if(!$this->bitinka_model->checkapi($api_key,$secret))\n		{\n			echo \"<h3 align='center'><font color='red'>Invalid API Key or Secret Key</font></h3>\";\n		}\n		else\n		{	\n			$getUserid = $this->bitinka_model->getUserdetailbyapi($api_key);\n			if($method==\"getInfo\")\n			{\n				$checkedMethod = $this->bitinka_model->checkinfoMethod($api_key);\n				if($checkedMethod)\n				{\n					$fetchresult = $this->bitinka_model->fetchapibalance($getUserid);\n                                        $ordercount  = $this->bitinka_model->getActiveorders($getUserid);\n					$btc = $fetchresult->BTC;\n					$pen = $fetchresult->PEN;\n					$ars = $fetchresult->ARS;\n					$bob = $fetchresult->BOB;\n					$bsf = $fetchresult->BSF;\n					$usd = $fetchresult->USD;\n					$brl = $fetchresult->BRL;\n					$cop = $fetchresult->COP;\n					$test[]= array( 	'BTC'=>trim($btc),\n								'USD'=>trim($usd),\n								'PEN'=>trim($pen),\n								'ARS'=>trim($ars),\n								'BOB'=>trim($bob),\n								'BSF'=>trim($bsf),\n								'BRL'=>trim($brl),\n								'COP'=>trim($cop),\n								'open_orders'=>trim($ordercount),\n									);\n				}\n				else\n				{\n					$test[]=array('1'=>'error');\n					echo \"Access Denied.\";\n				}\n			}\n			else if($method==\"TransHistory\")\n			{\n				$checkedMethod = $this->bitinka_model->checktradeMethod($api_key);\n				if($checkedMethod)\n				{\n					$TransHistory = $this->bitinka_model->getTranshistorys($getUserid);\n					foreach($TransHistory as $trans)\n					{	\n						$transtype 		= $trans->type;\n						$transcomment 	= $trans->comment;\n						$transdate 		= $trans->date;\n						$transtime 		= $trans->time;\n						$transstatus 	= $trans->status;\n						$test[]=array( 	'Type'=>trim($transtype),\n										'Comment'=>trim($transcomment),\n										'Date'=>trim($transdate),\n										'Time'=>trim($transtime),\n										'Status'=>trim($transstatus),\n												);\n					}\n				}\n				else\n				{\n                                        $test[]=array('1'=>'error');\n					echo \"Access Denied.\";\n				}\n			}\n			else if($method==\"TradeHistory\")\n			{\n				$checkedMethod = $this->bitinka_model->checktradeMethod($api_key);\n				if($checkedMethod)\n				{\n					$TradeHistory = $this->bitinka_model->getTradehistory($getUserid);\n					foreach($TradeHistory as $trade)\n					{	\n						$trade_orderId 		= $trade->trade_id;\n						$trade_type 			= $trade->Type;\n						$trade_firstCurrency 	= $trade->firstCurrency;\n						$trade_secondCurrency 	= $trade->secondCurrency;\n						$trade_amount 			= $trade->Amount;\n						$trade_date 			= $trade->orderDate;\n						$trade_time 			= $trade->orderTime;\n						$trade_status 			= $trade->status;\n						$pair = $trade_firstCurrency.\"_\".$trade_secondCurrency;\n						$test[]=array( 	'Order Id'=>trim($trade_orderId),\n									'Type'=>trim($trade_type),\n									'Pair'=>trim($pair),\n									'Amount'=>trim($trade_amount),\n									'Date'=>trim($trade_date),\n									'Time'=>trim($trade_time),\n									'Status'=>trim($trade_status),\n											);\n					}\n				}\n				else\n				{\n                                       $test[]=array('1'=>'error');\n					echo \"Access Denied.\";\n				}\n			}\n			else if($method==\"CancelOrder\")\n			{\n				$checkedMethod = $this->bitinka_model->checktradeMethod($api_key);\n				if($checkedMethod)\n				{\n					$CancelOrder = $this->bitinka_model->getCancelorders($getUserid);\n					if($CancelOrder)\n					{\n						foreach($CancelOrder as $cancel)\n						{	\n							$cancel_orderId 		= $cancel->trade_id;\n							$cancel_type 			= $cancel->Type;\n							$cancel_firstCurrency 	= $cancel->firstCurrency;\n							$cancel_secondCurrency 	= $cancel->secondCurrency;\n							$cancel_date 			= $cancel->orderDate;\n							$cancel_time 			= $cancel->orderTime;\n							$cancel_status 			= $cancel->status;\n							\n							$test[]=array( 	'Order Id'=>trim($cancel_orderId),\n										'Type'=>trim($cancel_type),\n										'Buy Currency'=>trim($cancel_firstCurrency),\n										'Sell Currency'=>trim($cancel_secondCurrency),\n										'Date'=>trim($cancel_date),\n										'Time'=>trim($cancel_time),\n										'Status'=>trim($cancel_status),\n												);\n						}\n					}\n					else\n					{\n						$test[]=array('1'=>'error');\n						   echo \"No Orders\";\n					}\n				}\n				else\n				{\n                                       $test[]=array('1'=>'error');\n					echo \"Access Denied.\";\n				}\n			}\n			else\n			{\n				$test[]=array('2'=>'error');\n				echo \"Method is not correct\";\n			}\n					$json=json_encode($test);\n					echo \"<pre>\";\n					print_r(json_decode($json));\n					echo \"</pre>\";\n		}\n	}\n	else\n	{\n		echo \"No data found\";\n	}\n} \n\nfunction createkeyName()\n{\n	$code1 = $this->bitinka_model->generateredeemString();\n	$code2 = $this->bitinka_model->generateredeemString();\n	$code3 = $this->bitinka_model->generateredeemString();\n	$code4 = $this->bitinka_model->generateredeemString();\n	$code5 = $this->bitinka_model->generateredeemString();\n	$secret = $this->bitinka_model->generatesecretString();\n	$api_key = $code1.\"-\".$code2.\"-\".$code3.\"-\".$code4.\"-\".$code5;\n	$keyname = $this->input->post('keyname');\n	$created = $this->bitinka_model->createApikey($api_key,$secret);\n	if($created)\n	{\n		$this->load->view('front/keys_page'); \n	}	\n}\n\nfunction enableApikey()\n{\n	$result = $this->bitinka_model->enableApikey();\n	if($result)\n	{\n		$this->load->view('front/keys_page'); \n	}\n}\n\nfunction disableApikey($id)\n{\n	$result = $this->bitinka_model->disableApikey($id);\n	if($result)\n	{\n		$this->load->view('front/keys_page'); \n	}\n}\n\nfunction refreshUSdbal()\n{\n	$client_userid		=	$this->session->userdata('client_userid');\n	$value = $this->bitinka_model->fetchuserbalancebyId($client_userid,\"USD\");\n	echo $value.\" USD\";\n}\n\nfunction refreshBtcbal()\n{\n	$client_userid		=	$this->session->userdata('client_userid');\n	$value = $this->bitinka_model->fetchuserbalancebyId($client_userid,\"BTC\");\n	echo $value.\" BTC\";\n}\n\nfunction refresh()\n{\n	$client_userid		=	$this->session->userdata('client_userid');\n	$currency_session	=	$this->session->userdata('bitinka_currency_session');  \n	if($currency_session==\"\")\n	{\n		$currency_session	=	\"btc_usd\";\n	}\n	$exp = explode('_',$currency_session);\n	$firstCurrency  = strtoupper($exp[0]);\n	$secondCurrency = strtoupper($exp[1]);\n	$Firstbal 	= $this->bitinka_model->fetchuserbalancebyId($client_userid,$firstCurrency);\n	$Secondbal 	= $this->bitinka_model->fetchuserbalancebyId($client_userid,$secondCurrency);\n	if($secondCurrency==\"BRL\") { $secondCurrency =\"BRL\"; }\n	$lowAsk 	= $this->bitinka_model->lowestaskprice($firstCurrency,$secondCurrency);\n	\n	$determined 	= $this->bitinka_model->predeterminedprice();\n	if($lowAsk==\"\")\n	{\n		if($currency_session==\"btc_usd\")\n		{\n			$lowAsk = $determined->btcusd_low;\n		}\n		else if($currency_session==\"btc_ars\")\n		{\n			$lowAsk = $determined->btcars_low;\n		}\n		else if($currency_session==\"btc_pen\")\n		{\n			$lowAsk = $determined->btcpen_low;\n		}\n		else if($currency_session==\"btc_bob\")\n		{\n			$lowAsk = $determined->btcbob_low;\n		}\n		else if($currency_session==\"btc_brl\")\n		{\n			$lowAsk = $determined->btcbrl_low;\n		}\n		else if($currency_session==\"btc_cop\")\n		{\n			$lowAsk = $determined->btccop_low;\n		}\n		else // bsf\n		{\n			$lowAsk = $determined->btcbsf_low;\n		}\n	}\n	$highBid = $this->bitinka_model->highestbidprice($firstCurrency,$secondCurrency);\n	if($highBid==\"\")\n	{\n		if($currency_session==\"btc_usd\")\n		{\n			$highBid = $determined->btcusd_high;\n		}\n		else if($currency_session==\"btc_ars\")\n		{\n			$highBid = $determined->btcars_high;\n		}\n		else if($currency_session==\"btc_pen\")\n		{\n			$highBid = $determined->btcpen_high;\n		}\n		else if($currency_session==\"btc_bob\")\n		{\n			$highBid = $determined->btcbob_high;\n		}\n		else if($currency_session==\"btc_brl\")\n		{\n			$highBid = $determined->btcbrl_high;\n		}\n		else if($currency_session==\"btc_cop\")\n		{\n			$highBid = $determined->btccop_high;\n		}\n		else\n		{\n			$highBid = $determined->btcbsf_high;\n		}\n	}\n	$USdbal = $this->bitinka_model->fetchuserbalancebyId($client_userid,\"USD\");\n	$Btcbal = $this->bitinka_model->fetchuserbalancebyId($client_userid,\"BTC\");\n	if($secondCurrency==\"BRL\") { $secondCurrency = \"BRL\"; } 	\n	echo $Firstbal.\" \".$firstCurrency.\",\".$Secondbal.\" \".$secondCurrency.\",\".$lowAsk.\" \".$secondCurrency.\",\".$highBid.\" \".$secondCurrency.\",\".$USdbal.\" USD,\".$Btcbal.\" BTC,\";\n}\n\nfunction refreshprice()\n{\n	$currency_session	=	$this->session->userdata('currency');  \n	if($currency_session==\"\")\n	{\n		$currency_session	=	\"btc_usd\";\n	}\n	$exp = explode('_',$currency_session);\n	$firstCurrency  = strtoupper($exp[0]);\n	$secondCurrency = strtoupper($exp[1]);\n	$lowAsk = $this->bitinka_model->lowestaskprice($firstCurrency,$secondCurrency);\n	if($lowAsk==\"\")\n	{\n		if($currency_session==\"btc_usd\")\n		{\n			$lowAsk=600;\n		}\n		else if($currency_session==\"btc_rur\")\n		{\n			$lowAsk=21500.107;\n		}\n		else if($currency_session==\"btc_eur\")\n		{\n			$lowAsk=412.90413;\n		}\n		else if($currency_session==\"ltc_btc\")\n		{\n			$lowAsk=0.02725;\n		}\n		else if($currency_session==\"ltc_usd\")\n		{\n			$lowAsk=16.7;\n		}\n		else if($currency_session==\"ltc_rur\")\n		{\n			$lowAsk=587;\n		}\n		else if($currency_session==\"ltc_eur\")\n		{\n			$lowAsk=11.4;\n		}\n		else if($currency_session==\"nmc_btc\")\n		{\n			$lowAsk=0.00484;\n		}\n		else if($currency_session==\"nmc_usd\")\n		{\n			$lowAsk=2.792;\n		}\n		else if($currency_session==\"nvc_btc\")\n		{\n			$lowAsk=0.01086;\n		}\n		else if($currency_session==\"nvc_usd\")\n		{\n			$lowAsk=6.25;\n		}\n		else if($currency_session==\"ppc_btc\")\n		{\n			$lowAsk=0.00472;\n		}\n		else if($currency_session==\"btc_ars\")\n		{\n			$lowAsk=4934;\n		}\n		else if($currency_session==\"btc_pen\")\n		{\n			$lowAsk=1705;\n		}\n		else if($currency_session==\"btc_bob\")\n		{\n			$lowAsk=4192.9880;\n		}\n		else\n		{\n			$lowAsk=3818.4148;\n		}\n	}\n	$highBid = $this->bitinka_model->highestbidprice($firstCurrency,$secondCurrency);\n	if($highBid==\"\")\n	{\n		if($currency_session==\"btc_usd\")\n		{\n			$highBid=598;\n		}\n		else if($currency_session==\"btc_rur\")\n		{\n			$highBid=21300.107;\n		}\n		else if($currency_session==\"btc_eur\")\n		{\n			$highBid=410.90413;\n		}\n		else if($currency_session==\"ltc_btc\")\n		{\n			$highBid=0.01725;\n		}\n		else if($currency_session==\"ltc_usd\")\n		{\n			$highBid=14.7;\n		}\n		else if($currency_session==\"ltc_rur\")\n		{\n			$highBid=585;\n		}\n		else if($currency_session==\"ltc_eur\")\n		{\n			$highBid=10.4;\n		}\n		else if($currency_session==\"nmc_btc\")\n		{\n			$highBid=0.00284;\n		}\n		else if($currency_session==\"nmc_usd\")\n		{\n			$highBid = 1.792;\n		}\n		else if($currency_session==\"nvc_btc\")\n		{\n			$highBid = 0.01072;\n		}\n		else if($currency_session==\"nvc_usd\")\n		{\n			$highBid = 4.25;\n		}\n		else if($currency_session==\"ppc_btc\")\n		{\n			$highBid = 0.00252;\n		}\n		else if($currency_session==\"btc_ars\")\n		{\n			$highBid = 4934;\n		}\n		else if($currency_session==\"btc_pen\")\n		{\n			$highBid = 1705;\n		}\n		else if($currency_session==\"btc_bob\")\n		{\n			$highBid = 4192.9880;\n		}\n		else if($currency_session==\"btc_ars\")\n		{\n			$highBid = 4934;\n		}\n		else if($currency_session==\"btc_pen\")\n		{\n			$highBid = 1705;\n		}\n		else if($currency_session==\"btc_bob\")\n		{\n			$highBid = 4192.9880;\n		}\n		else\n		{\n			$highBid = 3818.4148;\n		}\n	}\n	echo $lowAsk.\",\".$highBid;\n}\n\nfunction refreshsellbuyorders()\n{\n	$this->load->view('front/order');\n}\n\nfunction fail()\n{\n	$client_userid	=	$this->session->userdata('client_userid');  \n	if($client_userid)\n	{\n		$data['value']	=	\"Error while trying to deposit money\";\n	}\n	else\n	{\n		$data['value']	=	\"Error: you need to be logged in\";\n	}\n	$this->load->view('front/status',$data);\n}\n\nfunction pmsuccess()\n{\n	$batchNumber 	= $_REQUEST['PAYMENT_BATCH_NUM'];\n	$paymentAmount 	= $_REQUEST['PAYMENT_AMOUNT'];\n	$paymentUnits 	= $_REQUEST['PAYMENT_UNITS'];\n	/* $client_userid	=	$this->session->userdata('client_userid');  \n	if($client_userid)\n	{ */\n		$result = $this->bitinka_model->updatePMdetails($batchNumber,$paymentAmount,$paymentUnits);\n		$data['value']	=	\"Your amount succesfully deposited.\";\n	/* }\n	else\n	{\n		$data['value']	=	\"Error: you need to be logged in\";\n	} */\n	$this->load->view('front/status',$data);\n}\n\nfunction pmstatus()\n{\n	$client_userid	=	$this->session->userdata('client_userid');  \n	if($client_userid)\n	{\n		$data['value']	=	\"Status\";\n	}\n	else\n	{\n		$data['value']	=	\"Error: you need to be logged in\";\n	}\n	$this->load->view('front/status',$data);\n}\n\nfunction usersfun()\n{\n	echo $this->bitinka_model->fetchOnlineusers();\n}\n\nfunction botsfun()\n{\n	echo $this->bitinka_model->fetchBots();\n}\nfunction temp()\n{\n	$this->load->view('front/temp');\n}\nfunction checktfa()\n{\n	echo $tfa_result=$this->bitinka_model->checktfa();\n}\nfunction codegenerating()\n{\n		echo $result=$this->bitinka_model->codegenerating_update();\n}\nfunction get_comment()\n{\n	echo $result = $this->bitinka_model->insert_comment();\n}\n/* Crypto Currency Sections Ends Here */\n\nfunction ajaxuserverification1upload()\n{\n	$rnumber	=	mt_rand(0,999999); \n	$image		=	$rnumber.\"_\".$_FILES['verification1_doc']['name'];\n	$lext = substr($image , strrpos($image,'.')+ 1);\n	$imagen     = explode('.',$_FILES['verification1_doc']['name']);\n	$extension  = end($imagen);\n	$peso       = $_FILES['verification1_doc']['size'];\n	\n	if(($lext == \"png\") || ($lext == \"jpg\") || ($lext == \"JPEG\") || ($lext == \"jpeg\") || ($lext == \"JPG\") || ($lext == \"PNG\") || ($lext == \"pdf\") || ($lext == \"PDF\"))  {\n		\n		$query     = \"SELECT * FROM config_servers WHERE nombre = 'SRV-IMAGENES'\";\n		$ejecucion = $this->db->query($query);\n		$resultado = $ejecucion->row_array();\n		\n		$client_userid = $this->session->userdata('client_userid');\n	\n		$conexion     = ssh2_connect($resultado['IP'], $resultado['PUERTO']);\n		$utenticacion = ssh2_auth_password($conexion, $resultado['USUARIO'], $resultado['CLAVE']);\n		ssh2_scp_send($conexion, $_FILES[\"verification1_doc\"][\"tmp_name\"], '/var/www/uploader/customers/documents/documento1_'.$client_userid.'.'.$extension, 0777);\n		\n	    echo $result	=	$this->bitinka_model->uploadverification1($client_userid.'.'.$extension);\n	}\n	else\n	{\n		echo \"failure\";\n	}\n}\nfunction ajaxuserverification2upload()\n{\n	$rnumber	=	mt_rand(0,999999); \n	$image		=	$rnumber.\"_\".$_FILES['verification2_doc']['name'];\n	$lext = substr($image , strrpos($image,'.')+ 1);\n	\n	$imagen     = explode('.',$_FILES['verification2_doc']['name']);\n	$extension  = end($imagen);\n	$peso       = $_FILES['verification2_doc']['size'];\n	\n	if(($lext == \"png\") || ($lext == \"jpg\") || ($lext == \"JPEG\") || ($lext == \"jpeg\") || ($lext == \"JPG\") || ($lext == \"PNG\") || ($lext == \"pdf\") || ($lext == \"PDF\"))  {\n	\n	    $query     = \"SELECT * FROM config_servers WHERE nombre = 'SRV-IMAGENES'\";\n		$ejecucion = $this->db->query($query);\n		$resultado = $ejecucion->row_array();\n		\n		$client_userid = $this->session->userdata('client_userid');\n	\n		$conexion     = ssh2_connect($resultado['IP'], $resultado['PUERTO']);\n		$utenticacion = ssh2_auth_password($conexion, $resultado['USUARIO'], $resultado['CLAVE']);\n		ssh2_scp_send($conexion, $_FILES[\"verification2_doc\"][\"tmp_name\"], '/var/www/uploader/customers/documents/documento2_'.$client_userid.'.'.$extension, 0777);\n	\n	echo $result	=	$this->bitinka_model->uploadverification2($client_userid.'.'.$extension);\n	}\n	else\n	{\n		echo \"failure\";\n	}\n}\nfunction ajaxuserverification3upload()\n{\n	$rnumber	=	mt_rand(0,999999); \n	$image		=	$rnumber.\"_\".$_FILES['