$this->input->post('recaptcha');\n			if($captcha_code!=$recaptcha)\n			{\n				$this->session->set_flashdata('error',\"The captcha code does not match!\");\n				redirect('bitinka/register','referesh');		\n			}\n			else\n			{\n				$username		=	$this->input->post('username');\n				$username_already	=	$this->bitinka_model->checkusername($username);\n				if($username_already==\"already\")\n				{\n					$this->session->set_flashdata('error',\"Username is Already Exists\");\n					redirect('bitinka/register','referesh');\n				}\n				else\n				{\n					$email		=	$this->input->post('emailid');\n					$already	=	$this->bitinka_model->get_alreadyuserdeatils($email);\n					if($already==\"already\")\n					{\n						$this->session->set_flashdata('error',\"Email ID is Already Exists\");\n						redirect('bitinka/register','referesh');\n					}\n					else\n					{					\n						$result	=	$this->bitinka_model->add_userdetails();\n						$this->session->set_flashdata('success',\"Please Check Your Email and Activate Your Account\");\n						redirect('bitinka/register','referesh');						\n					}\n				}\n			}\n		}\n	}	\n} \n\n//returns username is exist or not	\nfunction ajax_checkuserid() \n{    \n	$search_data	=	$this->input->post('string');          \n	$message	=	$this->bitinka_model->ajax_checkuserid_model($search_data);         \n	if($message)\n	{         \n		echo \"<span style='color:red;font-weight:bold;font-size:13px;'>Someone already has that username. Try another?</span>\"; \n	}\n	else\n	{   \n		echo \"nothing\";\n	}\n}\n\n//returns email id is exist or not		\nfunction ajax_erralreadyemailid() \n{    \n	$es_emailid	=	$this->input->post('emailid');              \n	$email_err	=	$this->bitinka_model->ajaxget_alreadyuserdeatils($es_emailid);         \n	if($email_err)\n	{         \n		echo \"Email ID already exist\";\n	}\n}\n\n// send activation link to user\nfunction activationlink($id)\n{		\n	$result =$this->bitinka_model->activatedlink($id);\n	if($result) \n	{\n		$error	=	$this->session->set_flashdata('success',\"Your Account Has Been Activated Successfully\");	\n		redirect('bitinka/home','refresh');\n	}\n	else\n	{	\n		$error	=	$this->session->set_flashdata('error',\"Your account has been activated successfully Please Check Your Email and login into Your Account\");		\n		redirect('bitinka/home','refresh');	\n	}\n}\n\nfunction login()     \n{  \n	$client_userid		=	$this->session->userdata('client_userid');\n	if($client_userid==\"\")\n	{\n		$this->load->view('front/login'); \n	}\n	else\n	{\n		redirect('bitinka/home','referesh');\n		//$this->load->view('front/index'); \n	} \n} \n\n// returns login status\nfunction login_status()  \n\n{   \n	$res_login	=	$this->bitinka_model->check_logindetails();   	\n	echo $res_login;\n}\n\nfunction login_status1()  \n{ \n	$code=\"\";\n	$res_config= $this->bitinka_model->check_tfa_siteconfig();\n	if($res_config==\"enable\")\n	{\n		$res_admin	=	$this->bitinka_model->check_tfa_admin();\n		$code = $res_admin->randcode;\n		if($code==\"enable\")\n		{\n			$res_login	=	$this->bitinka_model->check_logindetails1();   	\n			if($res_login==\"active\")\n			{\n				echo \"enable\";\n			}\n			else\n			{\n				echo $res_login; \n			}\n		}\n		else\n		{	\n			$res_login	=	$this->bitinka_model->check_logindetails();   	\n			echo $res_login;\n		}\n	}\n	else\n	{\n		$res_login	=	$this->bitinka_model->check_logindetails();   	\n		echo $res_login;\n	}\n}\n\n// to view forgot password page\nfunction forgotpassword()     \n{   \n	$this->load->view('front/forgetpassword'); \n}  \n\n// ajax form for forgot password\nfunction ajaxforgotform()  \n{   \n	$res_login	=	$this->bitinka_model->forgot_passmail();   	\n	echo $res_login;\n}\n\n// from mail to function\nfunction forgot($id) //step 2\n{\n	$this->session->set_userdata('forgotid',$id);\n	redirect('bitinka/reset_password','refresh');	\n} \n\nfunction reset_password()\n{\n	$forgotid	=	$this->session->userdata('forgotid');\n	$data['id']	=	base64_decode($forgotid);\n	$this->load->view('front/resetpassword',$data);\n}\n\n// reset password form\nfunction ajaxreset_password()\n{\n	$result	=	$this->bitinka_model->reset_password_model();   \n	if($result)\n	{\n		echo \"success\";\n	}\n	else\n	{\n		echo \"failure\";\n	}\n}\n\nfunction news()     \n{   \n	$data['news_result']	=	$this->bitinka_model->fetchtotalnews();\n	$this->load->view('front/news',$data); \n} \n\nfunction single($id)\n{\n	$this->session->set_userdata('sess_newsid',$id);\n	redirect('bitinka/single_news','refresh');\n}\n\nfunction single_news()\n{\n	$newsid		=	$this->session->userdata('sess_newsid');\n	$perpage 	= 	$this->bitinka_model->getrowsperpage(); \n	$urisegment	=	$this->uri->segment(3);\n	$result		=	$this->bitinka_model->fetchparticularnewscomments($perpage,$urisegment,$newsid);    \n	if($result)  \n	{     \n		$total_rows = 	$this->bitinka_model->fetchparticularnewscommentscount($newsid);   \n		$base		=	\"single_news\";            \n		$this->pageconfig($total_rows,$base);         \n		$data['singlenews_result']	=	$result;\n		$data['top_news']			=	$this->bitinka_model->fetchtopnews();\n		$data['news_row']			=	$this->bitinka_model->fetchparticularnews($newsid);\n		$this->load->view('front/single_news',$data);		\n	}	\n	else\n	{	\n		$data['singlenews_result']	= \"\";\n		$data['top_news']			=	$this->bitinka_model->fetchtopnews();\n		$data['news_row']			=	$this->bitinka_model->fetchparticularnews($newsid);\n		$data['notfound']=$this->lang->line('no message are found ');       \n		$this->load->view('front/single_news',$data);		  \n	}\n}\nfunction faq()     \n{\n	$data['result']  =  $this->bitinka_model->get_faq_questions();	\n	$this->load->view('front/faq',$data); \n}  \n\nfunction terms()     \n{  \n	$result			=	$this->bitinka_model->fetchterms();\n	if($result)\n	{\n		foreach($result as $list)\n		{\n			$data['pageid'] = $list->pageid;\n			$data['pagetitle'] = $list->pagetitle;\n			$data['pagecontent'] = $list->pagecontent;\n			$data['description'] = $list->description;\n		}\n	}\n	else\n	{\n		$data['status'] = \"Deactiveted page\";\n	}\n	$this->load->view('front/terms',$data); \n}  \n\nfunction logout() \n{\n	$client_userid	=	$this->session->userdata('client_userid');    \n	$sessionvar		=	$this->session->userdata('client_mailid');  \n	if($sessionvar==\"\") \n	{	\n		$data['client_userid']='';	\n		$this->session->sess_destroy();\n		redirect('bitinka/login','refresh');\n	}  \n	else \n	{  \n		$m = new bitinka_model();\n		$m->logout_log($client_userid);\n		$this->session->sess_destroy();		  \n		$data['client_userid']='';  \n		redirect('bitinka/login','refresh');\n	} \n}\n\n// returns login status\nfunction changepwd_status()  \n{   \n	$result	=	$this->bitinka_model->changepwd_statusdetails();   	\n	echo $result;\n}\n\nfunction profile()\n{\n	$client_mailid		=	$this->session->userdata('client_mailid'); \n	$client_userid		=	$this->session->userdata('client_userid'); \n	if(($client_mailid==\"\") && ($client_userid==\"\") )\n	{   \n		redirect('bitinka/home','refresh'); 	 \n	}\n	else\n	{ \n		$this->load->view('front/profile');\n	}\n}\n\nfunction funds()\n{\n	$client_mailid		=	$this->session->userdata('client_mailid'); \n	$client_userid		=	$this->session->userdata('client_userid'); \n	\n	if(($client_mailid==\"\") && ($client_userid==\"\") )\n	{   \n		redirect('bitinka/home','refresh'); 	 \n	}\n	else\n	{ \n		$this->load->view('front/funds');\n	}\n}\n\nfunction edit()\n{\n	$client_mailid		=	$this->session->userdata('client_mailid'); \n	$client_userid		=	$this->session->userdata('client_userid'); \n	if(($client_mailid==\"\") && ($client_userid==\"\") )\n	{   \n		redirect('bitinka/home','refresh'); 	 \n	}\n	else\n	{ \n		$data['profile']	=	$this->bitinka_model->getprofilepicturestatus();\n		$this->load->view('front/edit',$data);\n	}\n}\n\nfunction security_old()\n{\n	$client_mailid		=	$this->session->userdata('client_mailid'); \n	$client_userid		=	$this->session->userdata('client_userid'); \n	if(($client_mailid==\"\") && ($client_userid==\"\") )\n	{   \n		redirect('bitinka/home','refresh'); 	 \n	}\n	else\n	{ \n		require_once 'GoogleAuthenticator.php';\n		$ga = new PHPGangsta_GoogleAuthenticator();\n		$user_result = $this->bitinka_model->user_check_tfa();\n		$user_secret = $this->bitinka_model->get_secret($client_mailid);\n		if($user_result==\"enable\" || $user_secret!=\"\")\n		{\n			$secret_code = $this->bitinka_model->get_secret($client_mailid); \n			$data['secret_code'] = $secret_code;\n			$data['url'] = $ga->getQRCodeGoogleUrl('Bitinka', $secret_code);\n			echo $data['url'];\n			exit;\n		}\n		else\n		{\n			 $result 		= 	$this->bitinka_model->get_tfacode();\n			 if($result)\n			 {\n				$data['secret_code'] 	= 	$result['secret'];\n				$data['onecode'] 		= 	$result['oneCode'];\n				$data['url']     		= 	$result['qrCodeUrl'];\n			 }\n			 else\n			 {\n				$data['secret_code'] 	= 	\"\";\n				$data['onecode'] 		= 	\"\";\n				$data['url']     		= 	\"\";\n			 }\n		}\n		$this->load->view('front/security',$data);\n	}\n}\n\nfunction security()\n{\n	$client_mailid		=	$this->session->userdata('client_mailid'); \n	$client_userid		=	$this->session->userdata('client_userid'); \n	if(($client_mailid==\"\") && ($client_userid==\"\") )\n	{   \n		redirect('bitinka/home','refresh'); 	 \n	}\n	else\n	{ \n		require_once 'GoogleAuthenticator.php';\n		$ga = new PHPGangsta_GoogleAuthenticator();\n\n		$secret = $this->bitinka_model->get_secret($client_mailid);\n		if($secret!='')\n		{\n			$secret_code = $secret;\n		}\n		else\n		{\n			$secret_code = $ga->createSecret(); \n		} \n		$data['secret_code'] = $secret_code;\n		$data['url'] = $ga->getQRCodeGoogleUrl('Bitinka', $secret_code);\n		$this->load->view('front/security',$data);\n	}\n}\n\nfunction trade_history()\n{\n	$client_mailid		=	$this->session->userdata('client_mailid'); \n	$client_userid		=	$this->session->userdata('client_userid'); \n	if(($client_mailid==\"\") && ($client_userid==\"\") )\n	{   \n		redirect('bitinka/home','refresh'); 	 \n	}\n	else\n	{ \n		$perpage 	= 	$this->bitinka_model->getrowsperpage(); \n		$urisegment	=	$this->uri->segment(3);\n		$result		=	$this->bitinka_model->fetchtradehistory($perpage,$urisegment);    \n		if($result)  \n		{     \n			$total_rows = $this->bitinka_model->fetchtradehistorycount();   \n			$base=\"trade_history\";            \n			$this->pageconfig($total_rows,$base);         \n			$data['trade_result']	=	$result;\n			$this->load->view('front/trade_history',$data);		\n		}	\n		else\n		{	\n			$data['trade_result']	= \"\";\n			$data['notfound']=$this->lang->line('No Trade History are found');        \n			$this->load->view('front/trade_history',$data);		  \n		}\n	}\n}\nfunction pendeposhow()\n{\n	$client_mailid		=	$this->session->userdata('client_mailid'); \n	$client_userid		=	$this->session->userdata('client_userid'); \n	if(($client_mailid==\"\") && ($client_userid==\"\") )\n	{   \n		redirect('bitinka/home','refresh'); 	 \n	}\n	else\n	{ \n		$perpage 	= 	$this->bitinka_model->getrowsperpage(); \n		$urisegment	=	$this->uri->segment(3);\n		$result		=	$this->bitinka_model->pendeposiit($perpage,$urisegment);    \n		if($result)  \n		{     \n			$total_rows = $this->bitinka_model->fetchpenhistorycount();   \n			$base=\"PEN Deposit History\";            \n			$this->pageconfig($total_rows,$base);         \n			$data['pen_deposit']	=	$result;\n			$this->load->view('front/pendeposhow',$data);		\n		}	\n		else\n		{	\n			$data['pen_deposit']	= \"\";\n			$data['notfound']=$this->lang->line('No PEN deposit are found'); ;        \n			$this->load->view('front/pendeposhow',$data);		  \n		}\n	}\n}\nfunction dninumberrequest()\n{  \n	echo $this->bitinka_model->dninumberrequest();   \n}\nfunction transaction_history()\n{\n	$client_mailid		=	$this->session->userdata('client_mailid'); \n	$client_userid		=	$this->session->userdata('client_userid'); \n	if(($client_mailid==\"\") && ($client_userid==\"\") )\n	{   \n		redirect('bitinka/home','refresh'); 	 \n	}\n	else\n	{ \n		$perpage 	= 	$this->bitinka_model->getrowsperpage(); \n		$urisegment	=	$this->uri->segment(3);\n		$result		=	$this->bitinka_model->fetchtransactionhistory($perpage,$urisegment);    \n		if($result)  \n		{     \n			$total_rows = $this->bitinka_model->fetchtransactionhistorycount();   \n			$base=\"transaction_history\";            \n			$this->pageconfig($total_rows,$base);         \n			$data['transaction_result']	=	$result;\n			$this->load->view('front/transaction_history',$data);		\n		}	\n		else\n		{	\n			$data['transaction_result']	= \"\";\n			$data['notfound']=$this->lang->line('No Transaction History are found'); ;        \n			$this->load->view('front/transaction_history',$data);		  \n		}\n	}\n}\n\n\nfunction order_history()\n{\n	$client_mailid		=	$this->session->userdata('client_mailid'); \n	$client_userid		=	$this->session->userdata('client_userid'); \n	if(($client_mailid==\"\") && ($client_userid==\"\") )\n	{   \n		redirect('bitinka/home','refresh'); 	 \n	}\n	else\n	{ \n		$perpage 	= 	$this->bitinka_model->getrowsperpage(); \n		$urisegment	=	$this->uri->segment(3);\n		$result		=	$this->bitinka_model->fetchorderhistory($perpage,$urisegment);    \n		if($result)  \n		{     \n			$total_rows = $this->bitinka_model->fetchorderhistorycount();   \n			$base=\"order_history\";            \n			$this->pageconfig($total_rows,$base);         \n			$data['order_result']	=	$result;\n			$this->load->view('front/order_history',$data);		\n		}	\n		else\n		{	\n			$data['order_result']	= \"\";\n			$data['notfound']=$this->lang->line('No Order History are found'); ;        \n			$this->load->view('front/order_history',$data);		  \n		}\n	}\n}\n\nfunction ajaxpairfororder()\n{	\n	$keyword	=	$this->input->post('keyword');\n	$pairs	=	strtoupper($keyword);\n	$result	=	$this->bitinka_model->ajaxpairforordermodel();\n	if($result)\n	{\n		?><table class=\"table table-striped\">\n<thead>\n			<tr>\n			  <th>Pair</th>\n			  <th>Type</th>\n			  <th>Amount</th>\n			  <th>Price</th>\n			  <th>Total</th>\n			   <th>Filled %</th>\n			  <th>Status</th>\n			  <th>Actions</th>\n			</tr>\n		  </thead>\n		  <tbody>\n		  <?php	foreach($result as $trade)	\n		{	 	\n			$tradetradeId 			= $trade->trade_id;\n			$tradeuserId 			= $trade->userId;\n			$tradePrice 			= $trade->Price;\n			$tradeAmount 			= $trade->Amount;\n			$tradeType 				= $trade->Type;\n			$tradeTotal 			= $trade->Total;\n			$tradefirstCurrency 	= $trade->firstCurrency;\n			$tradesecondCurrency 	= $trade->secondCurrency;\n			$tradestatus		 	= $trade->status;\n			$orderDate 				= $trade->orderDate;\n			$orderTime 				= $trade->orderTime;\n			$pair = $tradefirstCurrency.\"/\".$tradesecondCurrency;\n			\n			$activefilledAmount = $this->bitinka_model->checkOrdertempdetails($tradetradeId,$tradeType);\n			if($activefilledAmount)\n			{\n				$activefilledAmount = $tradeAmount-$activefilledAmount;\n			}\n			else\n			{\n				$activefilledAmount = $tradeAmount;\n			}\n			$activeCalcTotal = $activefilledAmount*$tradePrice;\n			$filled = $activefilledAmount/$tradeAmount*100;\n?>\n		   <tr>\n				<td><b><?php echo $pair; ?></b></td>\n				<?php if($tradeType==\"Buy\") { ?>\n				<td><span style='color:#008000;font-weight:bold;'><?php echo $tradeType; ?></span></td>\n				<?php	}	else	{	?>\n				<td><span style='color:#FF0000;font-weight:bold;'><?php echo $tradeType; ?></span></td>\n				<?php } ?>\n				<td><?php echo $activefilledAmount; ?></td>\n				<td><?php echo $tradePrice; ?></td>\n				<td><?php echo $activeCalcTotal; ?></td>\n				<td><?php echo $filled.\"%\"; ?></td>\n				<td><?php echo $tradestatus; ?></td>\n				<?php if($tradestatus!='filled' || $tradestatus!='cancelled') { ?>\n				<td><?php echo anchor('bitinka/close_active_order/'.$tradetradeId,'Cancel',array('style'=>\"color: red;\")); ?></td>\n				<?php } ?>\n			</tr>\n			<?php	}	?>\n		  </tbody>\n		</table>\n	<?php\n	}\n	else\n	{\n		echo \"<span style='color:red;font-weight: bold;font-size: 15px;'>No order history at the moment for $pairs pair</span>\";\n	}\n}\n\nfunction ajaxtypefororder()\n{	\n	$keyword	=	$this->input->post('keyword');\n	$types	=	strtoupper($keyword);\n	$result	=	$this->bitinka_model->ajaxtypeforordermodel();\n	if($result)\n	{\n		?><table class=\"table table-striped\">\n<thead>\n			<tr>\n			  <th>Pair</th>\n			  <th>Type</th>\n			  <th>Amount</th>\n			  <th>Price</th>\n			  <th>Total</th>\n			   <th>Filled %</th>\n			  <th>Status</th>\n			  <th>Actions</th>\n			</tr>\n		  </thead>\n		  <tbody>\n		  <?php	foreach($result as $trade)	\n		{	 	\n			$tradetradeId 			= $trade->trade_id;\n			$tradeuserId 			= $trade->userId;\n			$tradePrice 			= $trade->Price;\n			$tradeAmount 			= $trade->Amount;\n			$tradeType 				= $trade->Type;\n			$tradeTotal 			= $trade->Total;\n			$tradefirstCurrency 	= $trade->firstCurrency;\n			$tradesecondCurrency 	= $trade->secondCurrency;\n			$tradestatus		 	= $trade->status;\n			$orderDate 				= $trade->orderDate;\n			$orderTime 				= $trade->orderTime;\n			$pair = $tradefirstCurrency.\"/\".$tradesecondCurrency;\n			\n			$activefilledAmount = $this->bitinka_model->checkOrdertempdetails($tradetradeId,$tradeType);\n			if($activefilledAmount)\n			{\n				$activefilledAmount = $tradeAmount-$activefilledAmount;\n			}\n			else\n			{\n				$activefilledAmount = $tradeAmount;\n			}\n			$activeCalcTotal = $activefilledAmount*$tradePrice;\n			$filled = $activefilledAmount/$tradeAmount*100;\n?>\n		   <tr>\n				<td><b><?php echo $pair; ?></b></td>\n				<?php if($tradeType==\"Buy\") { ?>\n				<td><span style='color:#008000;font-weight:bold;'><?php echo $tradeType; ?></span></td>\n				<?php	}	else	{	?>\n				<td><span style='color:#FF0000;font-weight:bold;'><?php echo $tradeType; ?></span></td>\n				<?php } ?>\n				<td><?php echo $activefilledAmount; ?></td>\n				<td><?php echo $tradePrice; ?></td>\n				<td><?php echo $activeCalcTotal; ?></td>\n				<td><?php echo $filled.\"%\"; ?></td>\n				<td><?php echo $tradestatus; ?></td>\n				<?php if($tradestatus!='filled' || $tradestatus!='cancelled') { ?>\n				<td><?php echo anchor('bitinka/close_active_order/'.$tradetradeId,'Cancel',array('style'=>\"color: red;\")); ?></td>\n				<?php } ?>\n			</tr>\n			<?php	}	?>\n		  </tbody>\n		</table>\n	<?php\n	}\n	else\n	{\n		echo \"<span style='color:red;font-weight: bold;font-size: 15px;'>No order history at the moment for $types type</span>\";\n	}\n}\n\nfunction ajaxstatusfororder()\n{	\n	$keyword	=	$this->input->post('keyword');\n	$result	=	$this->bitinka_model->ajaxstatusforordermodel();\n	if($result)\n	{\n		?><table class=\"table table-striped\">\n<thead>\n			<tr>\n			  <th>Pair</th>\n			  <th>Type</th>\n			  <th>Amount</th>\n			  <th>Price</th>\n			  <th>Total</th>\n			   <th>Filled %</th>\n			  <th>Status</th>\n			  <th>Actions</th>\n			</tr>\n		  </thead>\n		  <tbody>\n		  <?php	foreach($result as $trade)	\n		{	 	\n			$tradetradeId 			= $trade->trade_id;\n			$tradeuserId 			= $trade->userId;\n			$tradePrice 			= $trade->Price;\n			$tradeAmount 			= $trade->Amount;\n			$tradeType 				= $trade->Type;\n			$tradeTotal 			= $trade->Total;\n			$tradefirstCurrency 	= $trade->firstCurrency;\n			$tradesecondCurrency 	= $trade->secondCurrency;\n			$tradestatus		 	= $trade->status;\n			$orderDate 				= $trade->orderDate;\n			$orderTime 				= $trade->orderTime;\n			$pair = $tradefirstCurrency.\"/\".$tradesecondCurrency;\n			\n			$activefilledAmount = $this->bitinka_model->checkOrdertempdetails($tradetradeId,$tradeType);\n			if($activefilledAmount)\n			{\n				$activefilledAmount = $tradeAmount-$activefilledAmount;\n			}\n			else\n			{\n				$activefilledAmount = $tradeAmount;\n			}\n			$activeCalcTotal = $activefilledAmount*$tradePrice;\n			$filled = $activefilledAmount/$tradeAmount*100;\n\n?>\n		   <tr>\n				<td><b><?php echo $pair; ?></b></td>\n				<?php if($tradeType==\"Buy\") { ?>\n				<td><span style='color:#008000;font-weight:bold;'><?php echo $tradeType; ?></span></td>\n				<?php	}	else	{	?>\n				<td><span style='color:#FF0000;font-weight:bold;'><?php echo $tradeType; ?></span></td>\n				<?php } ?>\n				<td><?php echo $activefilledAmount; ?></td>\n				<td><?php echo $tradePrice