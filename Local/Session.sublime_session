ure\";\n	}\n}\n\nfunction api_key()\n{\n	$client_mailid		=	$this->session->userdata('client_mailid'); \n	$client_userid		=	$this->session->userdata('client_userid'); \n	if(($client_mailid==\"\") && ($client_userid==\"\") )\n	{   \n		redirect('bitinka/home','refresh'); 	 \n	}\n	else\n	{ \n		$this->load->view('front/api_key');\n	}\n}\n\nfunction meta_trader()\n{\n	$client_mailid		=	$this->session->userdata('client_mailid'); \n	$client_userid		=	$this->session->userdata('client_userid'); \n	if(($client_mailid==\"\") && ($client_userid==\"\") )\n	{   \n		redirect('bitinka/home','refresh'); 	 \n	}\n	else\n	{ \n		$this->load->view('front/meta_trader');\n	}\n}\n \n/* Message Section Starts Here  */\nfunction compose()\n{\n	$client_mailid		=	$this->session->userdata('client_mailid'); \n	$client_userid		=	$this->session->userdata('client_userid'); \n	if(($client_mailid==\"\") && ($client_userid==\"\") )\n	{   \n		redirect('bitinka/home','refresh'); 	 \n	}\n	else\n	{ \n		$this->load->view('front/compose');\n	}\n} \n\nfunction inbox()\n{\n	$client_mailid		=	$this->session->userdata('client_mailid'); \n	$client_userid		=	$this->session->userdata('client_userid'); \n	if(($client_mailid==\"\") && ($client_userid==\"\") )\n	{   \n		redirect('bitinka/home','refresh'); 	 \n	}\n	else\n	{ \n		$perpage 	= 	$this->bitinka_model->getrowsperpage(); \n		$urisegment	=	$this->uri->segment(3);\n		$result		=	$this->bitinka_model->fetchinboxmessages($perpage,$urisegment);    \n		if($result)  \n		{     \n			$total_rows = $this->bitinka_model->fetchinboxmessagescount();   \n			$base=\"inbox\";            \n			$this->pageconfig($total_rows,$base);         \n			$data['inbox_result']	=	$result;\n			$this->load->view('front/inbox',$data);		\n		}	\n		else\n		{	\n			$data['inbox_result']	= \"\";\n			$data['notfound']=$this->lang->line('no message are found ');         \n			$this->load->view('front/inbox',$data);		  \n		}\n	}\n} \n\nfunction outbox()\n{\n	$client_mailid		=	$this->session->userdata('clie