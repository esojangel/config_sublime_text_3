l->updateAddress($createAddress,$currency);\n	?>\n		<div class=\"cls_main_block\">\n		<h3>Your address for deposit <?php echo $currency; ?>:</h3>\n		<div class=\"row-fluid\">\n		<div class=\"span5\">\n		<img src=\"<?php echo $rootUrl; ?>\">\n		<p><b><?php echo $createAddress; ?></b></p>\n		<a href=\"javascript:void(0);\">\n		<button class=\"btn btn-warning\" onclick=\"generateAddress('<?php echo $currency; ?>');\">Generate new address</button>\n		</a>\n		</div>\n		<div class=\"span6 pull-right\">\n		<ul class=\"deposit_rht_lst\">\n		<li>We do not have fee on <?php echo $currency; ?> deposits.</li>\n		<li>Minimal amount for deposit is - <b>0.0001 <?php echo $currency; ?></b>.</li>\n		<li>Your deposit will be credited in few minutes after <b>3 confirmations</b> on the <?php echo $currency_name; ?> network.</li>\n		<li>Transaction confirmation on the <?php echo $currency_name; ?> network can take <b>from 1 hour</b> and <b>up to 3 days</b> if you send it without fee.</li>\n		<li>Address can be used for further deposits.</li>\n		<li>We do not support <i>generated</i> transactions from pools like Eligius, P2Pool etc.</li>\n		</ul>\n		</div>\n		</div>\n		</div>\n	<?php	\n	}\n	else\n	{\n		echo \"unused\";\n	}\n}\n\nfunction withdrawcoin($currency)\n{\n	$data['balance'] = $this->bitinka_model->fetchCurrencyBalance('BTC');\n	$limitRow = $this->bitinka_model->get_withdrawlimit($currency);\n    $data['min'] 	= $limitRow->min_withdraw;\n	$data['fees'] 	= $limitRow->withdraw_fee;\n	$data['max'] 	= $limitRow->max_withdraw;\n	$data['currency'] = $currency;\n	$data['result'] = $this->bitinka_model->fetchVerifiedstatus();\n	$this->load->view('front/withdrawcoins',$data);\n}\n\nfunction withdrawcoinrequest() {\n    $check_userid	=	$this->session->userdata('client_userid'); \n	$m = new bitinka_model();\n	$request = $this->param();\n	$esCorrecto = $m->verificarPin($check_userid,$request[\"pin\"]);\n	if($esCorrecto){\n		$result	=	$m->withdrawcoinrequestmodel();\n		$this->success($result);\n	}else{\n		$this->error(\"Pin incorrecto\");\n	}\n}\nfunction redeemrequest()\n{\n	echo $result	=	$this->bitinka_model->redeemrequestmodel();\n}\nfunction redeem_confirm($token)\n{\n    $check_userid	=	$this->session->userdata('client_userid'); \n	$row            = $this->bitinka_model->checkredeemToken($token);\n	$redeemUserid 	= 	$row->userId;\n	$currency 	=	$row->currency;\n	$redeemstatus 	= 	$row->status;\n	if($check_userid==$redeemUserid)\n	{\n		if($redeemstatus==\"cancelled\" || $redeemstatus==\"sentcode\")\n		{\n			$data['value'] = \"Error: Application has already been confirmed or canceled earlier\";\n		}\n		else\n		{\n			$code1 = $this->bitinka_model->generateredeemString();\n			$code2 = $this->bitinka_model->generateredeemString();\n			$code3 = $this->bitinka_model->generateredeemString();\n			$code4 = $this->bitinka_model->generateredeemString();\n			$code5 = $this->bitinka_model->generateredeemString();\n			//BTCE-BTC-UMBQW2OU-KHBMG42V-FE0ROVSU-IGJ2F9KA-IU8N7GUB\n			$redeemcode = \"BITINKA-\".$currency.\"-\".$code1.\"-\".$code2.\"-\".$code3.\"-\".$code4.\"-\".$code5;\n			$updated = $this->bitinka_model->updateRedeemcode($redeemcode,$token);\n			if($updated)\n			{\n				$data['value'] = \"Your BITINKA Code: \".$redeemcode;\n			}\n		}\n	}\n	else\n	{\n		$data['value']	=	\"Error: you need to be logged in\";\n	}\n	$this->load->view('front/status',$data);\n}\n\nfunction redeem_cancel($token)\n{\n	$row = $this->bitinka_model->checkredeemToken($token);\n	$redeemUserid 	= 	$row->userId;\n	$redeemstatus 	= 	$row->status;\n	$client_userid	=	$this->session->userdata('client_userid');  \n	if($client_userid==$redeemUserid)\n	{\n		if($redeemstatus==\"cancelled\" || $redeemstatus==\"sentcode\")\n		{\n			$data['value'] = \"Error: Application has already been confirmed or cancelled earlier\";\n		}\n		else\n		{\n			$updated = $this->bitinka_model->cancelRedeem($token);\n			if($updated)\n			{\n				$data['value'] = \"Success: Your application has been cancelled successfully.\";\n			}\n		}\n	}\n	else\n	{\n		$data['value']	=	\"Error: you need to be logged in\";\n	}\n	$this->load->view('front/status',$data);\n}\n\nfunction withdraw_confirm_token($token)\n{\n	$row = $this->bitinka_model->checkconfirmation($token);\n	$Userid 	= 	$row->userId;\n	$Status 	= 	$row->status;\n	$client_userid	=	$this->session->userdata('client_userid');  \n	if($client_userid==$Userid)\n	{\n		if($Status==\"cancelled\" || $Status==\"confirmed\")\n		{\n			$data['value'] = \"Error: Application has already been confirmed or canceled earlier\";\n		}\n		else\n		{\n			$confirmResult 	= $this->bitinka_model->updateconfirmation($token);\n			$taken			=	$this->bitinka_model->withdraw_confirmmodel($token);\n			$amount 		= 	$taken->amount;\n			$purse 			= 	$taken->purse;\n			$currency		= 	$taken->currency;\n			if($currency==\"BTC\")\n			{\n				// $bitcoin_row 	= $this->fetchWallet('bitcoin');    // fetch bitinka wallet credentials\n				// $bitcoin_username	=	$bitcoin_row->username;\n				// $bitcoin_password	=	$bitcoin_row->password;\n				// $bitcoin_portnumber = 	$bitcoin_row->portnumber;\n				// $bitinka 	= new jsonRPCClient(\"http://$bitcoin_username:$bitcoin_password@127.0.0.1:$bitcoin_portnumber/\");\n				// $isvalid = $bitinka->sendtoaddress($purse,$amount);\n\n	\n				$btc_amount = $amount/0.00000001;\n				$user_wallet = $this->bitinka_model->get_userwallet();\n				$access_token = $this->bitinka_model->get_access_token();\n				$wallet_passphrase = $this->bitinka_model->get_walletpass();\n				\n				// $user_wallet = \"2MznkL8PEkkJH2raTYcrXMJNMmAGtE6psvn\";\n				// $btc_amount = 0.15;\n				// $purse = \"2N1RQisY6dyA85rLeeB57AX9hYDkcAAxwih\";\n\n				$output = exec('ACCESS_TOKEN=\"'.$access_token.'\";\n				BITGO_EXPRESS_HOST=\"localhost\";\n				WALLETID=\"'.$user_wallet.'\";\n				DESTINATIONADDRESS=\"'.$purse.'\";\n				AMOUNT='.$btc_amount.';\n				WALLETPASSPHRASE=\"'.$wallet_passphrase.'\";\n				curl -X POST \\\n				-H \"Content-Type: application/json\" \\\n				-H \"Authorization: Bearer $ACCESS_TOKEN\" \\\n				-d \"{ \\\"address\\\": \\\"$DESTINATIONADDRESS\\\", \\\"amount\\\": $AMOUNT, \\\"walletPassphrase\\\": \\\"$WALLETPASSPHRASE\\\" }\" \\\n				http://$BITGO_EXPRESS_HOST:3080/api/v1/wallet/$WALLETID/sendcoins');\n					$abc = json_decode($output);\n					// echo \"<pre>\";\n					// print_r($abc); exit;\n					$isvalid = $abc->hash;\n			}\n			$result 	= $this->bitinka_model->updateTransaction($isvalid,$token,$currency,$amount);\n			if($result)\n			{\n				$data['value']	=	$this->lang->line('Application successfully validated');\n			}\n		}\n	}\n	else\n	{\n		$data['value']	=	\"Error: you need to be logged in\";\n	}\n	$this->load->view('front/status',$data);\n}\n\nfunction withdraw_confirm($token)\n{\n	$row = $this->bitinka_model->checkconfirmation($token);\n	$Userid 	= 	$row->userId;\n	$askamount 	= 	$row->askamount;\n	$Status 	= 	$row->status;\n	$client_userid	=	$this->session->userdata('client_userid');  \n	if($client_userid==$Userid) {\n		$currentbalance = $this->bitinka_model->fetchuserbalancebyId($client_userid,'BTC');\n		// if($currentbalance>=$askamount)\n		// {\n			if($Status==\"cancelled\" || $Status==\"filled\")\n			{\n				$data['value'] = \"Error: Application has already been filled or canceled earlier\";\n			}\n			else\n			{\n				\n				$row = get_data('coin_withdraw',array('token'=>$token))->row();\n				$datetime = $row->request_date.\" \".$row->request_time;\n				$current = date('Y-m-d h:i:s');\n				$start_date = new DateTime($datetime);\n			    $end_date = new DateTime($current);\n				$interval = $start_date->diff($end_date);\n				$hours = $interval->h;\n\n				if($hours<1)\n				{\n					$confirmResult 	= 	$this->bitinka_model->updateconfirmation($token,$client_userid,$askamount);\n					// $confirmResult 	= 	$this->bitinka_model->updateconfirmation($token);antes\n					//esto va para la confirmacion del backend. author: jose angel\n					/*insert_data('withdraw_commission',array('userId'=>$client_userid,'currency'=>'BTC','amount'=>'0.001'));\n					$taken			=	$this->bitinka_model->withdraw_confirmmodel($token);\n					$ant 			= 	$taken->amount;\n					$amount 		= 	(float)$ant;\n					$purse 			= 	$taken->purse;\n					$currency		= 	$taken->currency;\n					if($currency==\"BTC\")\n					{\n						$bitcoin_row 	= $this->bitinka_model->fetchWallet('bitcoin');    // fetch bitinka wallet credentials\n						$bitcoin_username	=	$bitcoin_row->username;\n						$bitcoin_password	=	$bitcoin_row->password;\n						$bitcoin_portnumber = 	$bitcoin_row->portnumber;\n						$bitinka 	= new jsonRPCClient(\"http://$bitcoin_username:$bitcoin_password@127.0.0.1:$bitcoin_portnumber/\");\n						$isvalid = $bitinka->sendtoaddress($purse,$amount);\n					}\n					$result 	= $this->bitinka_model->updateTransaction($isvalid,$token,$currency,$amount);*///registra la transaccion de los BTC\n					$data['value']	=	$this->lang->line('Application successfully validated');\n				} else {\n					$data['value']	=	\"Your withdraw session time has been expired.\";\n				}\n			}\n		// }\n		// else\n		// {\n		// 	$data['value']	=	\"Error: Insufficient balance.\";\n		// }\n	}\n	else\n	{\n		$data['value']	=	\"Error: you need to be logged in\";\n	}\n	$this->load->view('front/status',$data);\n}\n\nfunction withdraw_cancel($token){\n	$row = $this->bitinka_model->checkconfirmation($token);\n	$Userid 	= 	$row->userId;\n	$Status 	= 	$row->status;\n	$askamount 	= 	$row->askamount;\n	$currency 	= 	$row->currency;\n	$client_userid	=	$this->session->userdata('client_userid');  \n	if($client_userid==$Userid)\n	{\n		if($Status==\"cancelled\" || $Status==\"filled\")\n		{\n			$data['value'] = \"Error: Application has already been filled or cancelled earlier\";\n		}\n		else\n		{\n			$updated = $this->bitinka_model->updatecancellation($token,$Userid,$currency,$askamount);\n			if($updated)\n			{\n				$data['value'] = \"Your application successfully cancelled.\";\n			}\n		}\n	}\n	else\n	{\n		$data['value']	=	\"Error: you need to be logged in\";\n	}\n	$this->