) || ($lext == \"jpg\") || ($lext == \"JPEG\") || ($lext == \"jpeg\") || ($lext == \"JPG\") || ($lext == \"PNG\") || ($lext == \"pdf\") || ($lext == \"PDF\"))  {\n		\n		$query     = \"SELECT * FROM config_servers WHERE nombre = 'SRV-IMAGENES'\";\n		$ejecucion = $this->db->query($query);\n		$resultado = $ejecucion->row_array();\n		\n		$client_userid = $this->session->userdata('client_userid');\n		\n	    $conexion     = ssh2_connect($resultado['IP'], $resultado['PUERTO']);\n		$utenticacion = ssh2_auth_password($conexion, $resultado['USUARIO'], $resultado['CLAVE']);\n		ssh2_scp_send($conexion, $_FILES[\"verification6_doc\"][\"tmp_name\"], '/var/www/uploader/customers/documents/documento6_'.$client_userid.'.'.$extension, 0777);\n		\n	    echo $result	=	$this->bitinka_model->uploadverification6($client_userid.'.'.$extension);\n		\n	}\n	else\n	{\n		echo \"failure\";\n	}\n}\n\nfunction sendMoneyfun()\n{\n	$sendemailid 	= $this->input->post('sendemailid');\n	$sendamount  	= $this->input->post('amount');\n	$from_Currency  = $this->input->post('from_Currency');\n	$to_Currency  	= $this->input->post('to_Currency');\n	$receiveAmount  = $this->input->post('receiveAmount');\n	$mainbalance  	= $this->input->post('mainbalance');\n	$min  			= $this->input->post('min');\n	$btc_yuan  	    = $this->input->post('btc_yuan');\n	\n	$tipo_envio = $this->input->post('key');\n	\n	\n	//print_r($_POST);\n	//exit();	\n	\n   //$min  	= \"0.001\";\n	$checkEmailid	= $this->bitinka_model->get_alreadyuserdeatils($sendemailid);\n	\n	if($sendamount>$mainbalance)\n	{\n		echo \"insufficient\";\n	}\n	else if($sendamount<$min)\n	{\n		echo \"minimum\";\n	}\n	else if($checkEmailid==\"nothing\")\n	{\n		echo \"nothing\";\n	}\n	else\n	{\n		echo $this->bitinka_model->sendMoneyfunModelNew($receiveAmount,$tipo_envio);\n	}\n}\n\nfunction confirmsend_money($token,$id_usuario = NULL)\n{\n	$client_userid	=	$this->session->userdata('client_userid'); \n	$sessionvar		=	$this->session->userdata('client_mailid');  \n	$row            =   $this->bitinka_model->chequear_token($token);\n	\n	$row2           = 	$this->bitinka_model->checkToken($token);\n	$row3           =   $this->bitinka_model->checkTokenCoinOrder($token);\n	\n	if($id_usuario != NULL){\n		$client_userid = $id_usuario;\n		$this->session->set_userdata('client_userid', $id_usuario);\n	}\n	\n	\n	if($row3['CODIGO'] == 1){\n	\n	if($row['CODIGO'] == 1)\n	{	\n	\n		$instant_id 	= 	$row2->instant_id;\n		$sentuserid 	= 	$row2->sentuserid;\n		$fromCurrency 	= 	$row2->fromCurrency;\n		$toCurrency 	= 	$row2->toCurrency;\n		$status 		= 	$row2->status;\n		\n		if($sentuserid==$client_userid)\n		{\n			\n			if($status==\"cancel\" ||$status==\"complete\")\n			{\n				$data['value'] = \"Error: Application has already been confirmed or canceled earlier\";\n			}\n			else\n			{ \n			\n				if($fromCurrency==$toCurrency) {\n				$this->bitinka_model->directMethod($token); } else\n				if($fromCurrency!=\"BTC\" && $toCurrency==\"BTC\") {\n				$this->bitinka_model->createBuyorder_forsm($token); } else\n				if($fromCurrency==\"BTC\" && $toCurrency!=\"BTC\") {\n				$this->bitinka_model->createSellorder_forsm($token); } else\n				if($fromCurrency!=$toCurrency) {     $firstresult =\n				$this->bitinka_model->createBuyorder_forsm($token); }\n				\n				\n				if($row2->toCurrency == 'RMB'){\n					\n					$this->cliente_webservice->enviar_btc_china($row2->recuserid,$row2->sellamount,$client_userid,$token,$row2->toAmount);\n					\n				}\n				\n				$data['value'] = \"Your send money order is processing.\";\n				\n				\n					\n			}\n		}\n		else\n		{\n			$data['value']	=	\"Error: you need to be logged in\";\n		}\n	}\n	else \n	{\n		\n		\n		\n		if($row['CODIGO'] == 4)\n	    {	\n		    \n			$fromAmount   = $row2->fromAmount;\n			$id_pago      = $row2->id_pago;\n			$fromCurrency =	$row2->fromCurrency;\n			\n			$this->bitinka_model->updateMoneycancel($token,$client_userid,$fromCurrency,$fromAmount,$id_pago);\n			\n		}\n		\n		$data['value'] = $row['MENSAJE'];//\"Error: your token mismatch\";\n	}\n	\n	}else{\n		\n		$data['value'] = $row3['MENSAJE'];\n		\n	}\n	\n	$this->load->view('front/status',$data);\n}\n\nfunction cancelsend_money($token,$id_usuario = NULL)\n{\n	\n	$client_userid	=	$this->session->userdata('client_userid');    \n	$sessionvar		=	$this->session->userdata('client_mailid');  \n	$row            = $this->bitinka_model->checkToken($token);\n	\n	if($id_usuario != NULL){\n		$client_userid = $id_usuario;\n	}\n	\n	\n	if($row)\n	{\n	$instant_id 	= 	$row->instant_id;\n	$sentuserid 	= 	$row->sentuserid;\n	$rescuserid 	= 	$row->recuserid;\n	$amount 		= 	$row->toAmount;\n	$fromAmount 	= 	$row->fromAmount;\n	$toCurrency 	= 	$row->toCurrency;\n	$fromCurrency 	= 	$row->fromCurrency;\n	$status 		= 	$row->status;\n	$id_pago 		= 	$row->id_pago;\n	$message 		= 	$row->message;\n	\n	\n	if($sentuserid==$client_userid)\n	{\n	if($status==\"cancel\" ||$status==\"complete\")\n	{\n	$data['value'] = \"Error: Application has already been confirmed or canceled earlier\";\n	}\n	else\n	{\n		\n		\n	$this->bitinka_model->updateMoneycancel($token,$sentuserid,$fromCurrency,$fromAmount,$id_pago);\n	$data['value'] = \"Success: Your application has been canceled successfully.\";\n	}\n	}\n	else\n	{\n	$data['value']	=	\"Error: you need to be logged in\";\n	}\n	}\n	else \n	{\n	$data['value']	=	\"Error: your token mismatch\";\n	}\n	$this->load->view('front/status',$data);\n}\n\nfunction receiveMoneyfun()\n{\n	$recemailid 	= $this->input->post('recemailid');\n	$recamount  	= $this->input->post('recamount');\n	$min  	= \"0.001\";\n	$checkEmailid	= $this->bitinka_model->get_alreadyuserdeatils($recemailid);\n	if($recamount<$min)\n	{\n		echo \"minimum\";\n	}\n	else if($checkEmailid==\"nothing\")\n	{\n		echo \"nothing\";\n	}\n	else\n	{\n		echo $this->bitinka_model->receiveMoneyfunModel();\n	}\n}\n\n/*--------------------poopandian e-curruncy fund transfer function start------------------*/\nfunction sendManyusd()\n{\n	$sendemailid 	= $this->input->post('sendemailidusd');\n	$sendamount  	= $this->input->post('sendamountusd');\n	$min  	= \"0.001\";\n	$checkEmailid	= $this->bitinka_model->get_alreadyuserdeatils($sendemailid);\n	$coinRow 		= $this->bitinka_model->fetchuserbalance();\n	$btcbalance 	= $coinRow->USD;\n	if($sendamount>$btcbalance)\n	{\n		echo \"insufficient\";\n	}\n	else if($sendamount<$min)\n	{\n		echo \"minimum\";\n	}\n	else if($checkEmailid==\"nothing\")\n	{\n		echo \"nothing\";\n	}\n	else\n	{\n		echo $this->bitinka_model->sendManyusdModel();\n	}\n}\nfunction sendManypen()\n{\n	$sendemailid 	= $this->input->post('sendemailidpen');\n	$sendamount  	= $this->input->post('sendamountpen');\n	$min  	= \"0.001\";\n	$checkEmailid	= $this->bitinka_model->get_alreadyuserdeatils($sendemailid);\n	$coinRow 		= $this->bitinka_model->fetchuserbalance();\n	$btcbalance 	= $coinRow->PEN;\n	if($sendamount>$btcbalance)\n	{\n		echo \"insufficient\";\n	}\n	else if($sendamount<$min)\n	{\n		echo \"minimum\";\n	}\n	else if($checkEmailid==\"nothing\")\n	{\n		echo \"nothing\";\n	}\n	else\n	{\n		echo $this->bitinka_model->sendManypenModel();\n	}\n}\nfunction sendManyars()\n{\n	$sendemailid 	= $this->input->post('sendemailidars');\n	$sendamount  	= $this->input->post('sendamountars');\n	$min  	= \"0.001\";\n	$checkEmailid	= $this->bitinka_model->get_alreadyuserdeatils($sendemailid);\n	$coinRow 		= $this->bitinka_model->fetchuserbalance();\n	$btcbalance 	= $coinRow->ARS;\n	if($sendamount>$btcbalance)\n	{\n		echo \"insufficient\";\n	}\n	else if($sendamount<$min)\n	{\n		echo \"minimum\";\n	}\n	else if($checkEmailid==\"nothing\")\n	{\n		echo \"nothing\";\n	}\n	else\n	{\n		echo $this->bitinka_model->sendManyarsModel();\n	}\n}\nfunction sendManybob()\n{\n	$sendemailid 	= $this->input->post('sendemailidbob');\n	$sendamount  	= $this->input->post('sendamountbob');\n	$min  	= \"0.001\";\n	$checkEmailid	= $this->bitinka_model->get_alreadyuserdeatils($sendemailid);\n	$coinRow 		= $this->bitinka_model->fetchuserbalance();\n	$btcbalance 	= $coinRow->BOB;\n	if($sendamount>$btcbalance)\n	{\n		echo \"insufficient\";\n	}\n	else if($sendamount<$min)\n	{\n		echo \"minimum\";\n	}\n	else if($checkEmailid==\"nothing\")\n	{\n		echo \"nothing\";\n	}\n	else\n	{\n		echo $this->bitinka_model->sendManybobModel();\n	}\n}\nfunction sendManybsf()\n{\n	$sendemailid 	= $this->input->post('sendemailidbsf');\n	$sendamount  	= $this->input->post('sendamountbsf');\n	$min  	= \"0.001\";\n	$checkEmailid	= $this->bitinka_model->get_alreadyuserdeatils($sendemailid);\n	$coinRow 		= $this->bitinka_model->fetchuserbalance();\n	$btcbalance 	= $coinRow->BSF;\n	if($sendamount>$btcbalance)\n	{\n		echo \"insufficient\";\n	}\n	else if($sendamount<$min)\n	{\n		echo \"minimum\";\n	}\n	else if($checkEmailid==\"nothing\")\n	{\n		echo \"nothing\";\n	}\n	else\n	{\n		echo $this->bitinka_model->sendManybsfModel();\n	}\n}\nfunction sendManybrl()\n{\n	$sendemailid 	= $this->input->post('sendemailidbrl');\n	$sendamount  	= $this->input->post('sendamountbrl');\n	$min  	= \"0.001\";\n	$checkEmailid	= $this->bitinka_model->get_alreadyuserdeatils($sendemailid);\n	$coinRow 		= $this->bitinka_model->fetchuserbalance();\n	$btcbalance 	= $coinRow->BRL;\n	if($sendamount>$btcbalance)\n	{\n		echo \"insufficient\";\n	}\n	else if($sendamount<$min)\n	{\n		echo \"minimum\";\n	}\n	else if($checkEmailid==\"nothing\")\n	{\n		echo \"nothing\";\n	}\n	else\n	{\n		echo $this->bitinka_model->sendManybrlModel();\n	}\n}\n/*--------------------poopandian e-curruncy fund transfer function end-------------------*/\n\nfunction seeInstanthistory()\n{\n	$client_userid		=	$this->session->userdata('client_userid');\n	$result = $this->bitinka_model->getSendmoneydetails();\n	if($result)\n	{\n	?>\n	<div class=\"cls_trade_history\">\n	<table class=\"table table-striped\">\n	<thead>\n                <tr>\n                  <th width=\"10%\">S.No</th>\n                  <th width=\"10%\">Date</th>\n                  <th width=\"30%\">Description</th>\n                  <th width=\"10%\">Amount</th>\n                  <th width=\"10%\">Status</th>\n                </tr>\n              </thead>\n              <tbody>\n              	<?php $i=0;\n              	foreach($result as $taken) \n              	{  \n              		$i++;\n              		$fromAmount 	= $taken->fromAmount; \n              		$toAmount	 	= $taken->toAmount; \n              		$sentuserid 	= $taken->sentuserid; \n              		$recuserid 		= $taken->recuserid; \n              		$fromCurrency 	= $taken->fromCurrency; \n              		$toCurrency 	= $taken->toCurrency; \n              		$status 		= $taken->status; \n              		$created_date 	= $taken->created_date; \n              		$sentUsername = $this->bitinka_model->get_username($recuserid);\n              		$sentSymbol = \"- \";\n              		$recUsername = $this->bitinka_model->get_username($sentuserid);\n              		$recSymbol = \"+ \";\n              	?>\n               <tr>\n                  <td><?php echo $i; ?></td>\n                  <td><?php echo $created_date; ?></td>\n                  <?php if($client_userid==$sentuserid) { ?>\n                  <td><?php echo \"You sent $toCurrency to $sentUsername\"; ?></td>\n                  <td style=\"color: red; font-weight: bold;\"><?php echo $sentSymbol.$fromAmount.\" \".$fromCurrency; ?></td>\n                  <?php } else if($client_userid==$recuserid)  { ?>\n                  <td><?php echo \"You received $toCurrency from $recUsername\"; ?></td>\n                  <td style=\"color: green; font-weight: bold;\"><?php echo $recSymbol.$toAmount.\" \".$toCurrency; ?></td>\n                  <?php } ?>\n                  <td><?php echo $status; ?></td>\n                </tr>\n                <?php } ?>\n              </tbody>\n            </table>\n\n	</div>\n	<?php	\n	}\n	else\n	{ ?>\n		<div class=\"cls_main_block\" style=\"text-align: center; font-weight: bold;\">No Transaction History at the moment.</div>\n	<?php }\n}\n\n// New Send Money Section Starts Here\n\n\nfunction send_money()\n{\n	$client_mailid		=	$this->session->userdata('client_mailid'); \n	$client_userid		=	$this->session->userdata('client_userid'); \n	if(($client_mailid==\"\") && ($client_userid==\"\") )\n	{   \n		redirect('bitinka/home','refresh'); 	 \n	}\n	else\n	{ \n		$this->load->model('inkapay_model');\n		$data['result'] = $this->bitinka_model->fetchparticularsendmoneydetails();\n		$data['solicitudes'] = $this->inkapay_model->listSolicitudPagosHechos($client_userid);\n		$this->load->view('front/send_money',$data);\n	}\n}\n\n\nfunction receieve_money()\n{\n	$client_mailid		=	$this->session->userdata('client_mailid'); \n	$client_userid		=	$this->session->userdata('client_userid'); \n	if(($client_mailid==\"\") && ($client_userid==\"\") )\n	{   \n		redirect('bitinka/home','refresh'); 	 \n	}\n	else\n	{ \n		$this->load->view('front/receieve_money');\n	}\n}\n\nfunction getbtcvaluefun() \n{\n	$from_Currency 	= $this->input->post('from_Currency');\n	$to_Currency 	= \"BTC\";\n	$amount 		= $this->input->post('amount');\n\n	$amount 		= urlencode($amount);\n	$from_Currency 	= urlencode($from_Currency);\n	$to_Currency 	= urlencode($to_Currency);\n\n	if($from_Currency==\"PEN\" || $from_Currency==\"BOB\" ||  $from_Currency==\"BRL\")\n	{\n		$onebtcval = $this->bitpayapi($from_Currency);\n		$total = $amount/$onebtcval;\n		echo number_format((float)$total, 4, '.', '');\n	}\n	else\n	{\n	$url = \"http://api.bitcoinvenezuela.com/?html=no&currency=$from_Currency&amount=$amount&to=$to_Currency\";\n		//Curl  for execute the request url\n		$header[] = \"Accept: application/json\";\n		$header[] = \"Accept-Encoding: gzip\";\n		$ch = curl_init();\n		curl_setopt( $ch, CURLOPT_HTTPHEADER, $header );\n		curl_setopt($ch,CURLOPT_ENCODING , \"gzip\");\n		curl_setopt($ch, CURLOPT_CUSTOMREQUEST, 'GET');\n		curl_setopt( $ch, CURLOPT_URL, $url );\n		curl_setopt( $ch, CURLOPT_RETURNTRANSFER, true );\n		$response = json_decode(curl_exec($ch));\n		$rt =  curl_getinfo($ch);\n		echo number_format((float)$response, 4, '.', '');\n	}	\n}\n\nfunction toBtc($from_Currency=\"\",$zigzag=\"\") // buy orders for send money\n{\n	$from_Currency 	= $this->input->post('from_Currency');\n	$to_Currency 	= \"BTC\";\n	$amountUSD 		= $this->input->post('amount');\n	$currency_pair  = strtolower($to_Currency).\"_\".strtolower($from_Currency);\n	$determined 	= $this->bitinka_model->predeterminedprice();\n	$lowAsk 	    = $this->bitinka_model->lowestaskprice($to_Currency,$from_Currency);\n	$btn_key        = $this->input->post('key');\n	\n	\n	if($lowAsk==\"\")\n	{\n		if($currency_pair==\"btc_usd\")\n		{\n			$lowAsk = $determined->btcusd_low;\n		}\n		else if($currency_pair==\"btc_ars\")\n		{\n			$lowAsk = $determined->btcars_low;\n		}\n		else if($currency_pair==\"btc_pen\")\n		{\n			$lowAsk = $determined->btcpen_low;\n		}\n		else if($currency_pair==\"btc_bob\")\n		{\n			$lowAsk = $determined->btcbob_low;\n		}\n		else if($currency_pair==\"btc_brl\")\n		{\n			$lowAsk = $determined->btcbrl_low;\n		}\n		else if($currency_pair==\"btc_cop\")\n		{\n			$lowAsk = $determined->btccop_low;\n		}\n		else // bsf\n		{\n			$lowAsk = $determined->btcbsf_low;\n		}\n		\n	}\n    \n	$lowAsk = bcadd($lowAsk,(bcdiv(bcmul($lowAsk,0.25,2),100,2)),2);//nuevo:: aumentando valor de oferta en el precio para que se efectue mas rapido\n	//$lowAsk = $lowAsk+(($lowAsk*0.25)/100);\n\n	$sendMoneyrow   =    $this->bitinka_model->sendmoneySettings();\n	\n	\n	\n	if($btn_key== 'test' || $btn_key== 'order'){\n		$commission  =    $sendMoneyrow->commission/2;\n	}elseif($btn_key== 'test1' || $btn_key== 'order_exp'){//comision express\n		$commission  =    $sendMoneyrow->commission_express/2;\n	}\n\n	$readytotal = bcdiv($amountUSD,$lowAsk,8);//$amountUSD/$lowAsk;\n	\n	//$readytotal = $amountUSD/$lowAsk;\n	//echo $lowAsk.' | '.bcdiv($amountUSD,$lowAsk,8).' | '.$amountUSD/$lowAsk;exit();\n	\n	$readyfee 	= bcdiv(bcmul($readytotal,$commission,8),100,8);//($readytotal*$commission)/100;\n	\n	$fee 	= $readyfee;//number_format((float)$readyfee, 4, '.', '');\n	$total 	= $readytotal;//number_format((float)$readytotal, 4, '.', '');\n	\n	if($zigzag=='')\n	{\n		echo $output = $fee.\"@\".$total.\"@\".$lowAsk;\n	}\n	else\n	{\n		return $output = $fee.\"@\".$total.\"@\".$lowAsk;\n	}\n}\n\n	function prueba(){\n		print_r($this->cliente_webservice->prueba2());\n	}\n	\n	function prueba_mail(){\n		print_r($this->cliente_webservice->prueba_mail());\n	}\n\nfunction fromBtc($amount=\"\",$zigzag=\"\") // sell order for send money \n{\n	$from_Currency 	= \"BTC\";\n	$to_Currency 	= $this->input->post('to_Currency');\n	$btn_key        = $this->input->post('key');\n	\n	if($amount==\"\")\n	{\n		$amount 		= $this->input->post('amount');\n	}\n	$currency_pair  = strtolower($from_Currency).\"_\".strtolower($to_Currency);\n	$determined 	= $this->bitinka_model->predeterminedprice();\n	$highBid 		= $this->bitinka_model->highestbidprice($from_Currency,$to_Currency);\n	\n	if($highBid==\"\")\n	{\n		if($currency_pair==\"btc_usd\")\n		{\n			$highBid = $determined->btcusd_high;\n		}\n		else if($currency_pair==\"btc_ars\")\n		{\n			$highBid = $determined->btcars_high;\n		}\n		else if($currency_pair==\"btc_pen\")\n		{\n			$highBid = $determined->btcpen_high;\n		}\n		else if($currency_pair==\"btc_bob\")\n		{\n			$highBid = $determined->btcbob_high;\n		}\n		else if($currency_pair==\"btc_brl\")\n		{\n			$highBid = $determined->btcbrl_high;\n		}\n		else if($currency_pair==\"btc_cop\")\n		{\n			$highBid = $determined->btccop_high;\n		}\n		else if($currency_pair==\"btc_rmb\")\n		{\n			$highBid = $this->cliente_webservice->valor_alto_rmb();\n		}\n		else\n		{\n			$highBid = $determined->btcbsf_high;\n		}\n	}\n	\n	\n    $highBid = bcsub($highBid,(bcdiv(bcmul($highBid,0.25,2),100,2)),2);\n	\n	$sendMoneyrow       =    $this->bitinka_model->sendmoneySettings();\n	\n	if($btn_key== 'test' || $btn_key== 'order'  ){\n		$commission  =    $sendMoneyrow->commission/2;\n	}elseif($btn_key== 'test1' || $btn_key== 'order_exp'){//comision express\n		$commission  =    $sendMoneyrow->commission_express/2;\n		\n	}\n	$rate 		= bcdiv(bcmul($amount,$commission,8),100,8);//($amount*$commission)/100;\n	$readyfee 	= bcmul($rate,$highBid,2);//$rate*$highBid;\n	$total 		= bcmul($amount , $highBid,2);//$amount * $highBid\n	\n	//echo $amount.' X '.$highBid;exit();\n	if($currency_pair==\"btc_rmb\"){\n		\n		$btc_yuan = $this->cliente_webservice->valor_btc_yuan($total);\n		\n	}else{\n		\n		$btc_yuan = 0;\n		\n	}\n	\n	// $total = number_format((float)$total, 4, '.', '');\n	$fee = $readyfee;//number_format((float)$readyfee, 4, '.', '');\n	\n	if($zigzag=='')\n	{\n		echo $output = $fee.\"@\".$total.\"@\".$highBid.\"@\".$btc_yuan;\n	}\n	else\n	{\n		return $output = $fee.\"@\".$total.\"@\".$highBid.\"@\".$btc_yuan;\n	}\n}\n\nfunction zigzag()\n{\n	$from_Currency 	= $this->input->post('from_Currency');\n	$to_Currency 	= $this->input->post('to_Currency');\n	$amount 		= $this->input->post('amount');\n	$currency_pair  = strtolower($from_Currency).\"_\".strtolower($to_Currency);\n	$btn_key        = $this->input->post('key');\n\n	// assume usd to ars\n	$toArray 	= $this->toBtc($from_Currency,'zigzag');\n\n	$toArray 	= explode('@', $toArray);\n	$buyfee 	= $toArray['0'];\n	$buyValue 	= $toArray['1']; // btc amount\n	$buyPrice 	= $toArray['2'];\n    \n	$fromArray  = $this->fromBtc($buyValue,'zigzag');\n\n	$fromArray 	= explode('@', $fromArray);\n	$sellfee 	= $fromArray['0'];\n	$sellValue 	= $fromArray['1']; // ars amount\n	$sellPrice 	= $fromArray['2'];\n    $btc_yuan   = $fromArray['3'];\n	\n    $sendMoneyrow   =    $this->bitinka_model->sendmoneySettings();\n	//$commission     =    $sendMoneyrow->commission;\n	\n	if($btn_key== 'test' || $btn_key== 'order'){\n		$commission  =    $sendMoneyrow->commission;\n	}elseif($btn_key== 'test1' || $btn_key== 'order_exp'){//comision express\n		$commission  =    $sendMoneyrow->commission_express;\n	}	\n	\n	$sellfee = bcmul($sellValue,($commission/100),2);//la comision de 1 % \n	\n	echo $buyfee.\"@\".$buyValue.\"@\".$buyPrice.\"@\".$sellfee.\"@\".$sellValue.\"@\".$sellPrice.\"@\".$btc_yuan;\n}\n\nfunction createBuyorder_forsm()\n{\n	echo $this->bitinka_model->createBuyorder_forsm();\n}\n\nfunction mapping_forsm() \n{\n	$type         = $this->input->post('type');\n	$freshorderid = $this->input->post('freshorderid');\n	\n	$result = $this->bitinka_model->mapping($freshorderid);\n	if($result==\"empty\")\n	{\n		echo \"failure\";\n	}\n	else\n	{\n		echo \"success_\".$result;\n	}    \n}\n\nfunction get_currency2() \n{\n	$from_Currency 	= \"BTC\";\n	$to_Currency 	= $this->input->post('to_Currency');\n	$amount 		= $this->input->post('amount');\n\n	$amount 		= urlencode($amount);\n	$from_Currency 	= urlencode($from_Currency);\n	$to_Currency 	= urlencode($to_Currency);\n\n	if($to_Currency==\"VEF\") // to BSF\n	{\n		$btcusd     = $this->btcvenezuelaapi();\n		$dolartoday = $this->dolartodayapi();\n		$btcvsf = $btcusd*$dolartoday;\n		$total = $amount*$btcvsf;\n		echo number_format((float)$total, 4, '.', '');\n	}\n	else if($to_Currency==\"PEN\" || $to_Currency==\"BOB\" ||  $to_Currency==\"BRL\")\n	{\n		$onebtcval = $this->bitpayapi($to_Currency);\n		$total = $onebtcval*$amount;\n		echo number_format((float)$total, 4, '.', '');\n	}\n	else\n	{\n	$url = \"http://api.bitcoinvenezuela.com/?html=no&currency=$from_Currency&amount=$amount&to=$to_Currency\";\n		//Curl  for execute the request url\n		$header[] = \"Accept: application/json\";\n		$header[] = \"Accept-Encoding: gzip\";\n		$ch = curl_init();\n		curl_setopt( $ch, CURLOPT_HTTPHEADER, $header );\n		curl_setopt($ch,CURLOPT_ENCODING , \"gzip\");\n		curl_setopt($ch, CURLOPT_CUSTOMREQUEST, 'GET');\n		curl_setopt( $ch, CURLOPT_URL, $url );\n		curl_setopt( $ch, CURLOPT_RETURNTRANSFER, true );\n		$response = json_decode(curl_exec($ch));\n		$rt =  curl_getinfo($ch);\n		echo number_format((float)$response, 4, '.', '');\n	}\n}\n\nfunction bitpayapi($currency)\n{\n	$url = \"https://bitpay.com/api/rates\";\n	$header[] = \"Accept: application/json\";\n	$header[] = \"Accept-Encoding: gzip\";\n	$ch = curl_init();\n	curl_setopt( $ch, CURLOPT_HTTPHEADER, $header );\n	curl_setopt($ch,CURLOPT_ENCODING , \"gzip\");\n	curl_setopt($ch, CURLOPT_CUSTOMREQUEST, 'GET');\n	curl_setopt( $ch, CURLOPT_URL, $url );\n	curl_setopt( $ch, CURLOPT_RETURNTRANSFER, true );\n	$response = json_decode(curl_exec($ch));\n	$rt =  curl_getinfo($ch);\n	if($currency==\"PEN\")\n	{\n		$value = $response[107]->rate;\n	}\n	else if($currency==\"BOB\")\n	{\n		$value = $response[28]->rate;\n	}\n	else\n	{\n		$value = $response[37]->rate;\n	}\n	return $value;\n}\n\nfunction btcvenezuelaapi()\n{\n	$btcvenezuelaapi = \"http://api.bitcoinvenezuela.com/\";\n	//Curl  for execute the request url\n	$header[] = \"Accept: application/json\";\n	$header[] = \"Accept-Encoding: gzip\";\n	$ch = curl_init();\n	curl_setopt( $ch, CURLOPT_HTTPHEADER, $header );\n	curl_setopt($ch,CURLOPT_ENCODING , \"gzip\");\n	curl_setopt($ch, CURLOPT_CUSTOMREQUEST, 'GET');\n	curl_setopt( $ch, CURLOPT_URL, $btcvenezuelaapi );\n	curl_setopt( $ch, CURLOPT_RETURNTRANSFER, true );\n	$response = json_decode(curl_exec($ch));\n	$rt =  curl_getinfo($ch);\n	return $btcusd = $response->BTC->USD;\n}\n\nfunction dolartodayapi()\n{\n	$url = \"https://s3.amazonaws.com/dolartoday/data.json\";\n	$header[] = \"Accept: application/json\";\n	$ch = curl_init();\n	curl_setopt($ch, CURLOPT_URL,$url);\n	curl_setopt($ch, CURLOPT_RETURNTRANSFER, true);\n	curl_setopt($ch, CURLOPT_CUSTOMREQUEST, 'GET');\n	curl_setopt($ch, CURLOPT_SSL_VERIFYPEER, false);\n	curl_setopt($ch, CURLOPT_SSL_VERIFYHOST, 2);\n	$result = curl_exec($ch);\n\n	$response =  json_decode( preg_replace('/[\\x00-\\x1F\\x80-\\xFF]/', '', $result), true );\n	return $response['USD']['transferencia'];\n}\n\n// New Send Money Section Ends Here\n\nfunction nice()\n{\n	$dateofreg=\"2014-11-12 11:52:21\";\n	echo $this->bitinka_model->nicetime($dateofreg);\n}\n\nfunction edituserdetail()\n{\n	echo $result=$this->bitinka_model->edituserdetail();\n}\n\nfunction ajax_statedetails($countryid)\n{\n	$state_result = $this->bitinka_model->getdetailsofstate($countryid);  \n	echo '<div class=\"control-group \">\n	<label class=\"control-label span4\">State</label>\n	<div class=\"controls\">\n	<div class=\"input-prepend\">';			 \n	echo \"<select name='state' id='state' onchange='onState();'>\n	<option value='0' selected='selected'>-- Select State --</option>\";\n	foreach($state_result as $record)\n	{\n	echo \"<option value='\".$record->id.\"'>\".$record->state_name.\"</option>\"; \n	}\n	echo \"</select>\";\n	echo \"</div></div></div>\"; 		     \n}\nfunction makependepositviabcp()\n{\n	echo $this->bitinka_model->makependepositviabcp(); \n}\n\n\nfunction prueba_transferencia()\n{\n	$currency 	=	'USD';	\n	$tipoTransf =	$this->input->post('tipo_transf');	\n	\n	echo $this->bitinka_model->comisionDivisas($currency,$tipoTransf);	\n}\n\n\n\nfunction visa_mastercard_payment()\n{\n	$cardtype 		=	$this->input->post('card_type');	\n	$expiry_month 	=	$this->input->post('expiry_month');\n	$expiry_year 	=	$this->input->post('expiry_year');\n	$expiry_date 	=	$expiry_month.\"/\".$expiry_year;\n	// $city_name		=	$this->input->post('city');\n	// $state 			=	$this->input->post('state');\n	// $country 		=	$this->input->post('country');\n	// if($country>0)\n	// { \n	// 	$q = $this->bitinka_model->getcountry($country);\n	// 	$country_name = $q->country_name;\n	// } \n	// else\n	// {\n	// 	$country_name=\"\";\n	// }\n	// if($state>0)\n	// {\n	// 	$q1 = $this->bitinka_model->getstate($state);\n	// 	$state_name = $q1->state_name;\n	// } \n	// else\n	// {\n	// 	$state_name=\"\";\n	// }\n\n	// Authorize.net lib\n	$this->load->library('Authorize_net');     \n	$client_mailid	  = $this->session->userdata('client_mailid');\n	// $userRow = $this->bitinka_model->get_currentuserdetils($client_mailid);\n	// $firstname = $userRow->firstname;\n	// $lastname  = $userRow->lastname;\n	// $x_first_name =$firstname;\n	// $x_last_name  =$lastname;\n	// $x_address	  =$this->input->post('address');\n	// $x_city		  =$city_name;	 \n	// $x_state      =$state_name;\n	// $x_zip		  =$this->input->post('zipcode');\n	// $x_country    =$country_name;\n	$x_phone      =$this->input->post('phone');\n	$card_number   =$this->input->post('card_number');\n	$x_exp_date   =$expiry_date;\n	$x_card_code  =$this->input->post('security_code');\n	$amount       =$this->input->post('amount');\n\n	$auth_net = array(\n	'x_card_num'=> $card_number, \n	'x_exp_date'=> $x_exp_date,\n	'x_card_code'=> $x_card_code,\n	'x_description'=> 'A Test Transaction', \n	'x_amount'=> $amount,\n	// 'x_first_name'=> $x_first_name,\n	// 'x_last_name'=> $x_last_name,\n	// 'x_address'=> $x_address,\n	// 'x_city'=> $x_city, \n	// 'x_state'=> $x_state,\n	// 'x_zip'=> $x_zip,\n	// 'x_country'=> $x_country,\n	// 'x_phone'=> $x_phone,\n	'x_email'=> $client_mailid,\n	//'x_customer_ip'=> $this->input->ip_address(),\n	);  \n	if($cardtype == \"master\")  \n	{\n		$this->session->set_userdata('payment_method','Mastercard');\n		$pattern = \"/^([51|52|53|54|55]{2})([0-9]{14})$/\";//Mastercard\n		if (preg_match($pattern,$card_number)) \n		{\n		$verified = true;\n		}  \n		else  \n		{\n		$verified = false;\n		}\n	} \n	else if($cardtype == \"visa\")   \n	{\n		$this->session->set_userdata('payment_method','Visa');  \n		$pattern = \"/^([4]{1})([0-9]{12,15})$/\";//Visa\n		if (preg_match($pattern,$card_number)) \n		{\n		$verified = true;\n		} \n		else \n		{\n		$verified = false;\n		}\n	}	\n	else \n	{	\n		$verified = false; \n	}\n	if($verified==true)\n	{  \n		$this->authorize_net->setData($auth_net);  \n		// Try to AUTH_CAPTURE\n		if( $this->authorize_net->authorizeAndCapture() )\n		{\n			$transaction_id = $this->authorize_net->getTransactionId();\n			$approval_code  = $this->authorize_net->getApprovalCode();\n			$this->bitinka_model->completeCarddeposit($transaction_id,$approval_code); // insert card payment details\n			$data['transaction_id'] = $transaction_id;\n			$data['approval_code']  = $approval_code;\n			$this->load->view('front/pensuccess',$data);\n		}\n		else \n		{\n			$this->bitinka_model->cancelCarddeposit(); // cancel card payment details\n			$data['error'] = $this->authorize_net->getError();\n			$this->load->view('front/penfailure',$data);\n		}\n	}	\n	else\n	{\n		echo \"<center><h3>Please check your Card Number. It is invalid</h3></center>\";\n	}\n}\n\nfunction test() \n{\n	$url = \"https://bitpay.com/api/rates\";\n	//$url = \"https://s3.amazonaws.com/dolartoday/data.json\";\n	//Curl  for execute the request url\n	$header[] = \"Accept: application/json\";\n	$header[] = \"Accept-Encoding: gzip\";\n	$ch = curl_init();\n	curl_setopt( $ch, CURLOPT_HTTPHEADER, $header );\n	curl_setopt($ch,CURLOPT_ENCODING , \"gzip\");\n	curl_setopt($ch, CURLOPT_CUSTOMREQUEST, 'GET');\n	curl_setopt( $ch, CURLOPT_URL, $url );\n	curl_setopt( $ch, CURLOPT_RETURNTRANSFER, true );\n	$response = json_decode(curl_exec($ch));\n	$rt =  curl_getinfo($ch);\n	echo $response[28]->code.$response[28]->rate.\"<br/>\";\n	echo $response[37]->code.$response[37]->rate.\"<br/>\";\n	echo $response[107]->code.$response[107]->rate;\n	echo \"<pre>\";\n	print_r($response);\n}\n\nfunction nathan()\n{\n	$url = \"https://s3.amazonaws.com/dolartoday/data.json\";\n	$header[] = \"Accept: application/json\";\n	$ch = curl_init();\n	curl_setopt($ch, CURLOPT_URL,$url);\n	curl_setopt($ch, CURLOPT_RETURNTRANSFER, true);\n	curl_setopt($ch, CURLOPT_CUSTOMREQUEST, 'GET');\n	curl_setopt($ch, CURLOPT_SSL_VERIFYPEER, false);\n	curl_setopt($ch, CURLOPT_SSL_VERIFYHOST, 2);\n	$result = curl_exec($ch);\n\n	$response =  json_decode( preg_replace('/[\\x00-\\x1F\\x80-\\xFF]/', '', $result), true );\n	echo \"<pre>\";\n	print_r($response);\n	echo $response['USD']['transferencia'];\n}\n\nfunction trans()\n{\n	$this->load->view('front/trans');\n}\n\nfunction rough()\n{\n	$str = $this->input->post('str');\n	$this->session->set_userdata('googtrans',$str);\n}\n\nfunction newsletter()\n{\n	$newsemail = $this->input->post('email');\n	$result