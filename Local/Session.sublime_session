w_message()\n{\n	$client_mailid		=	$this->session->userdata('client_mailid'); \n	$client_userid		=	$this->session->userdata('client_userid'); \n	if(($client_mailid==\"\") && ($client_userid==\"\") )\n	{   \n		$this->session->set_userdata('login_error','Please login here.');      \n		redirect('bitinka/home','refresh');   \n	}\n	else\n	{\n		$id	=	$this->session->userdata('ses_view_msg'); \n		$fetchdata	=	$this->bitinka_model->get_individual_message($id);\n		if($fetchdata)\n		{\n			$read_data	=	$this->bitinka_model->assign_read_mail($id);\n			if($read_data)\n			{\n				$data['view_result']	=	$fetchdata;  \n				$this->load->view('front/view_message',$data);	\n			}\n		} \n	}\n}\n\nfunction notifications()\n{\n	$this->load->view('front/notifications');	\n}\n\n/* Message Section Ends Here  */\n\nfunction refreshmarketprice()\n{\n	$this->load->view('front/refreshfeed');\n}\n\nfunction ajaxupdatemarketprice()\n{\n	echo $result = $this->bitinka_model->ajaxupdatemarketprice();\n}\n\n// SUPPORT SYSTEM STARTS HERE\n\nfunction support_old()\n{\n	$client_mailid		=	$this->session->userdata('client_mailid'); \n	$client_userid		=	$this->session->userdata('client_userid'); \n	if(($client_mailid==\"\") && ($client_userid==\"\") )\n	{   \n		$this->session->set_userdata('login_error','Please login here.');      \n		redirect('bitinka/home','refresh');   \n	}\n	else\n	{\n		$data['know_general_questions']=$this->bitinka_model->know_gen_ques_count();\n		$data['know_api_questions']=$this->bitinka_model->know_api_ques_count();\n		$data['know_meta_questions']=$this->bitinka_model->know_meta_ques_count();\n		$data['resultupdates']=$this->bitinka_model->getall_support_update();\n		$this->load->view('front/tickets',$data);\n	}\n\n}\n\nfunction knowledge()\n{\n$client_mailid		=	$this->session->userdata('client_mailid'); \n	$client_userid		=	$this->session->userdata('client_userid'); \n	if(($client_mailid==\"\") && ($client_userid==\"\") )\n	{   \n		$this->session->set_userdata('login_error','Please 