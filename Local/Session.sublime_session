\n}\n\nfunction withdrawforbitinkacode()\n{\n	echo $result	=	$this->bitinka_model->withdrawforbitinkacode();\n}\n\n/* Crypto Currency Section Starts Here */\n\nfunction getCurrencyname($currency)\n{\n	if($currency==\"BTC\")\n	{ \n		$currency_name = \"Bitcoin\"; \n	}\n	else if($currency==\"LTC\")\n	{ \n		$currency_name = \"Litecoin\"; \n	}\n	else if($currency==\"NMC\")\n	{ \n		$currency_name = \"Namecoin\"; \n	}\n	else if($currency==\"NVC\")\n	{ \n		$currency_name = \"Novacoin\"; \n	}\n	else if($currency==\"TRC\")\n	{ \n		$currency_name = \"Teracoin\"; \n	}\n	else if($currency==\"PPC\")\n	{ \n		$currency_name = \"Peercoin\"; \n	}\n	else if($currency==\"FTC\")\n	{ \n		$currency_name = \"Feathercoin\"; \n	}\n	else if($currency==\"XPM\")\n	{ \n		$currency_name = \"Primecoin\"; \n	}\n	return $currency_name;\n}\n\nfunction depositcoin_bitgo($currency)\n{	\n	$client_mailid	=	$this->session->userdata('client_mailid'); \n	$currency_name 		= $this->getCurrencyname($currency);\n	$checkAddress 		= $this->bitinka_model->checkdepositAddress($currency);\n	$setaccount = $client_mailid;\n	if($checkAddress)\n	{ \n		$rootUrl = \"https://chart.googleapis.com/chart?cht=qr&chs=150x150&chl=$checkAddress&choe=UTF-8&chld=L\"; ?>\n		<div class=\"cls_main_block\">\n		<h3>Your address for deposit <?php echo $currency; ?>:</h3>\n		<div class=\"row-fluid\">\n		<div class=\"span5\">\n		<img src=\"<?php echo $rootUrl; ?>\">\n		<p><b><?php echo $checkAddress; ?></b></p>\n		<a href=\"javascript:void(0);\">\n		<button class=\"btn btn-warning\" onclick=\"generateAddress('<?php echo $currency; ?>');\">Generate new address</button>\n		</a>\n		</div>\n		<div class=\"span6 pull-right\">\n		<ul class=\"deposit_rht_lst\">\n		<li>We do not have fee on <?php echo $currency; ?> deposits.</li>\n		<li>Minimal amount for deposit is - <b>0.0001 <?php echo $currency; ?></b>.</li>\n		<li>Your deposit will be credited in few minutes after <b>3 confirmations</b> on the <?php echo $currency_name; ?> network.</li>\n		<l